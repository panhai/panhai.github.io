.Coupon{
    margin: 10px 10px 0;
    padding-bottom: 15px;
    background-color: #fff;
    border-radius: 3px;
    font-size: 14px;
}
.Coupon h2{
    padding: 0 13px;
    font-weight: normal;
    line-height: 42px;
    color: #262626;
    font-size: 15px;
    border-bottom: 1px solid #e2e2e2;
}
.Coupon p{
    color: #7f7f7f;
    padding: 0 13px;
}
.Coupon .blue{ 
    color: #0f7cd7;
}
.Coupon p strong{
    float: right;
    font-weight: normal;
}
.Coupon .font12{
    width: 92%;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    font-size: 12px;
}
.Coupon .font12 span{
    color: #3c3c3c;
}
.Coupon .mar_t10{
    margin-top: 10px;
}
.Coupon .tip{
    position: relative;
    padding-left: 10px;
    margin: 6px 13px 10px;
    height: 30px;
    line-height: 30px;
    background-color: #fff8d8;
    font-size: 11px;
    border: 1px solid #ebe1b2;
    
}
.Coupon .tip p{
    text-overflow: ellipsis;
    white-space: nowrap;
    overflow: hidden;
    padding: 0;
    color: #333;
}
.Coupon .tip::before,
.Coupon .tip::after{
    content:"";
    position: absolute;
    top: -8px;
    left: 10px;
    width: 0;
    height: 0;
    border-style: solid;
    border-width: 0 6px 8px 6px;
    border-color: transparent transparent #ebe1b2 transparent;

    -webkit-transform: skewX(-15deg);
       -moz-transform: skewX(-15deg);
        -ms-transform: skewX(-15deg);
         -o-transform: skewX(-15deg);
            transform: skewX(-15deg);
}
.Coupon .tip::after{
    left: 10px;
    top: -7px;
    border-bottom-width: 11px;
    border-color: transparent transparent #fff8d8 transparent;
}
.Coupon a{
    display: block;
    margin: 19px 13px 0;
    height: 36px;
    line-height: 36px;
    text-align: center;
    font-size: 12px;
    color: #3187da;
    border-radius: 3px;
    border: 1px solid #3187da;
}