/*内边距*/
.p_b40{ padding-bottom: 40px;}
.user{
    position: relative;
    height: 150px;
    background-color: #40a1f2;
    overflow: hidden;
}
.user .user_info{
    margin-top: 25px;
    display: -moz-box;
    display: -webkit-box;
    display: box;


    padding: 0 10px;
}
.user .user_info img{
    width: 50px;
    height: 50px;
    overflow: hidden;
    border-radius: 50%;
}
.user .user_info .user_info_txt{
    display: block;
    position: relative;
    padding-left: 10px;
    -moz-box-flex: 1;
    -webkit-box-flex: 1;
    box-flex: 1;
    font-size: 14px;
    color: #fff;
    border: 1px solid transparent;
}
.user .user_info .user_info_txt h2{
    font-weight: normal;
    font-size: 14px;
    margin-top: 7px;
}
.user .user_info .user_info_txt p{
    font-size: 12px;
    line-height: 20px;
}
.user .user_link{
    position: absolute;
    bottom: 0;
    width: 100%;
    display: -moz-box;
    display: -webkit-box;
    display: box;

    background-color: #3d95dd;
    text-align: center;
}
.user .user_link h4{
    font-weight: normal;
}
.user .user_link a{
    -moz-box-flex: 1;
    -webkit-box-flex: 1;
    box-flex: 1;
    display: block;
    padding-top: 8px;
    height: 42px;
    font-size: 12px;
    color: #fff;
    border-right: 1px solid #79bef7;
}
.user .user_link a:last-child{
    border-right: 0;
}
/*我的订单*/
.order{
    margin-top: 11px;
    background-color: #fff;
    border-top: solid 1px #dedede;
}
.userList{
    display: table;
    width: 100%;
    height: 44px;
}
.userList .userList_icon {
    width: 23px;
    height: 23px;
    display: table-cell;
    float: left;
    margin: 10px 10px 0 10px;
}
.userList .userList_icon img{
    width: 100%;
}
.userList .userList_txt{
    position: relative;
    display: table-cell;
    vertical-align: middle;
    width: 90%;
    font-size: 14px;
}
.userList .userList_txt a{
    color: #454545;
    display: block;
}
.userList .userList_txt a span{
    position: absolute;
    right: 10px;
    padding-right: 10px;
    font-size: 11px;
    color: #a4a4a4;
    line-height: 20px;
    height: 20px;
}
.line{ 
    border-bottom: solid 1px #f2f2f2;
}
.menu_list{
    display: -moz-box;
    display: -webkit-box;
    display: box;
    border-bottom: solid 1px #dedede;
    background-color: #fff;
}
.menu_list a{
    position: relative;
    display: block;

    -moz-box-flex: 1;
    -webkit-box-flex: 1;
    box-flex: 1;

    height: 60px;
    line-height: 85px;
    text-align: center;
    font-size: 12px;
    color: #4f4f4f;
}
.menu_list a.new span{
    position: absolute;
    z-index: 33;
    left: 52%;
    top: 7px;
    width: 15px;
    height: 15px;
    background-color: #fff;
    border: 2px solid #ff4400;
    box-sizing: border-box;
    border-radius: 50%;
    color: #ff4400;
    overflow: hidden;
    line-height: 15px;
    font-size: 8px;
}
.menu_list a img{
    position: absolute;
    left: 50%;
    top: 13px;
    margin-left: -10px;
    width: 21px;
    height: 16px;
}

.item{
    margin-top: 10px;
    background-color: #fff;
    border-top: solid 1px #dedede;
    border-bottom: solid 1px #dedede;

}

.fixed_bottom{
    display: -moz-box;
    display: -webkit-box;
    display: box;

    position: fixed;
    bottom: 0;
    width: 100%;
    height: 48px;
    border-top: solid 1px #dedede;
    background-color: #fff;
}
.fixed_bottom a{
    position: relative;
    display: block;

    -moz-box-flex: 1;
    -webkit-box-flex: 1;
    box-flex: 1;
    background-color: #fff;
    height: 48px;

    border-top: 1px solid transparent;
    text-align: center;
    line-height: 64px;
    font-size: 10px;
    color: #858585;
/*    background: url(../images/UCenter/nav_1.png) no-repeat center 6px;
    background-size: 21px 19px;*/
}
.fixed_bottom a p{
    line-height: 14px;
    color: #666;
}
.fixed_bottom a em{
    display: block;
    margin-top: 4px;
    font-size: 24px;
}
.fixed_bottom a.on em{
    color:#40a1f2 !important;
}
