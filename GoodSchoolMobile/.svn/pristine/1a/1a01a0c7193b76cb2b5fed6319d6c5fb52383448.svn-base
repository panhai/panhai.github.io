/*支付成功*/
.signup-ok ,.procedure-txt{ padding: 20px; background: #fff url("data:image/png;base64,") repeat-x bottom; background-size:12px;}
.signup-ok h2 { margin-left: 16px; margin-bottom: 15px; line-height: 46px; font-size: 18px; color: #2d2d2d; text-align: center; }
.signup-ok h2::before { content: ''; background: url("data:image/png;base64,") no-repeat center center; background-size: contain; width: 40px; height: 45px; margin-right: 14px; display: inline-block; vertical-align: bottom; }
.signup-ok p { clear: left; font-size: 14px; color: #686868; text-align: center; }
.see-order, .request { display: block; width: 93.76%; height: 3.166667rem; margin: 0 auto; border-radius: 3px; font-size: 14px; text-align: center; line-height: 3.166667rem; }
.see-order { background-color: #308df6; color: #ffffff; margin-bottom: 15px; margin-top: 28px; }
.request { background-color: #fff; color: #308df6; }

/*福利保障*/
.welfare { height: 45px; padding-right: 0.8rem; background-color: #fff; border-top: 1px solid #ececec; border-bottom: 1px solid #ececec; line-height: 45px; font-size: 13px; }
.welfare span { position: relative; padding-left: 16px; margin-left: 1rem; color: #636363; }
.welfare em { color: #c1c1c1; }

/*咨询状态*/
.consult-State { overflow: hidden; background-color: #fff; padding: 10px 0.8rem; }
.consult-State p { float: left; width: 88%; font-size: 12px; border-bottom: #ececec; overflow: hidden; text-overflow: ellipsis; white-space: nowrap; }
.consult-State p:first-child { color: #4b4b4b; }
.consult-State p:last-of-type { color: #989898; margin-top: 3px; }
.consult-State .icon-arrow-right { margin: 0; margin-top: -5px; margin-left: 20px; color: #c1c1c1; }

/*支付信息*/
.payment-Info { margin-top: 12px; padding: 6px 0; background-color: #fff; }
.payment-Info li { padding: 0 1rem; border-bottom: 1px dashed #ebebeb; height: 44px; line-height: 44px; font-size: 14px; clear: both; }
.payment-Info li:last-child { border: 0; font-size: 14px; color: #3f3f3f; }
.payment-Info .item-l { float: left; color: #3e3e3e; }
.payment-Info .item-l span:last-child { color: #ff6000; font-size: 22px; }
.payment-Info .item-l span:last-child em { font-size: 14px; }
.payment-Info .item-l i { font-style: normal; color: #34aaf5; font-weight: bold; font-size: 15px;}
.payment-Info .item-r { float: right; }
.payment-Info .item-r span { width: 80px; display: inline-block; text-align: center;color:#3e3e3e; }
.payment-Info .item-r a { display: inline-block; width: 80px; height: 32px; background-color: #fff; border: 1px solid #34aaf5; border-radius: 3px; line-height: 32px; text-align: center; color: #34aaf5; }

/*学校信息*/
.school-Info, .course-Info,.Discount { background-color: #fff; margin-top: 12px; }
.school-Info-in { overflow: hidden; }
.school-Info h2, .course-Info h2, .Discount h2 { height: 48px; padding: 0 1rem; line-height: 48px; border-bottom: 1px solid #ededed; font-size: 14px; color: #a4a4a4; font-weight: normal; }
.school-Info .school-addr { padding: 20px 0 20px 1rem; width: 80%; float: left; }
.school-Info .school-map { float: left; width: 15%; height: 40px; border-left: 1px solid #dcdcdc; margin-top: 20px; background: url(data:image/png;base64,) center center no-repeat; background-size: 21px 18px; }
.school-Info .school-addr p { max-width: 100%; overflow: hidden; text-overflow: ellipsis; white-space: nowrap; }
.school-Info .school-addr p:first-child { font-size: 13px; color: #2f2f2f; }
.school-Info .school-addr p:last-child { font-size: 12px; color: #8c8c8c; margin-top: 4px; }
.school-contact { padding: 0 3%; height: 67px; line-height: 67px; border-top: 1px solid #ededed; text-align: center; background-color: #fff; }
.school-contact a { display: inline-block; width: 137px; height: 35px; line-height: 35px; text-align: center; border: 1px solid #c2deff; border-radius: 10px; color: #308df6; }
.school-contact a:first-child::before, .school-contact a:last-child::before { content: ''; display: inline-block; width: 16px; height: 16px; background: url(data:image/png;base64,) no-repeat; background-size: contain; vertical-align: middle; margin-right: 3%; }
.school-contact a:last-child::before { background-image: url(data:image/png;base64,); }
.school-contact a:last-child { margin-left: 15px; }

/*课程详情*/
.course-Info h2 { height: 40px; line-height: 40px; }
.course-Info-in{ overflow: hidden;height: 200px;}
.course-Info-in h3 { font-size: 15px; color: #323232; margin-bottom: 14px; font-weight: normal; }
.course-Info-in h4 { font-size: 12px; color: #828282; margin-bottom: 8px; font-weight: normal; }
.course-Info-in { padding: 10px 1rem; }
.course-Info-in p { color: #303030; font-size: 14px; }
.course-more { background-color: #fff; height: 40px; line-height: 40px; border-top: 1px solid #ededed; margin-top: 10px; text-align: center; font-size: 12px; color: #909090; }
.course-more .icon-arrow-right{ float: none;}
/*联系人*/
.contact { background-color: #fff; margin-top: 12px; padding: 1rem; }
.contact img { width: 42px; height: 42px; border-radius: 50%; border: 1px solid #e7ecf2; float: left; margin-right: 1rem; }
.contact h3 { margin-top: 4px; font-size: 13px; color: #3e3e3e; }
.contact p { margin-top: 4px; font-size: 13px; color: #898989; }
.footer { display: -webkit-box; }
.footer a { -webkit-box-flex: 1; display: block; margin-top: 14px; }

/*退款*/
.refund-t { padding: 1rem; background-color: #fff; overflow: hidden;}
.refund-img { float: left; margin-right: 10px; width: 5.833333rem;height: 5.833333rem; overflow:hidden;}
.refund-img img{ width: 100%; border-radius: 4px;}
.refund-txt h2{ font-size: 1.333333rem; color:#2f2f2f;font-weight: normal;  margin-bottom: 0.666667rem; line-height: 1.916667rem; overflow: hidden; text-overflow: ellipsis; white-space: nowrap;}
.refund-txt p{ font-size: 1.2rem; color:#959595; margin-top: 0.333333rem;}
.refund{ margin-top: 4px;margin-bottom: 30px;}
.refund-txt p:last-child span{ color: #ff6102;}
.refund li { line-height: 3.75rem; font-size:14px;}
.refund li:not(.info-item) { padding: 0 10px; height: 3.2rem; line-height: 4rem; color: #838383; }
.refund li { border-bottom: 1px solid #ececec; }
.refund li .info-item-r { position: relative; }
.refund li .info-item-r.on::before { border: 0; background: url("data:image/png;base64,") no-repeat; background-size: contain; }
.refund li .info-item-r::before { content: ''; display: inline-block; width: 20px; height: 20px; border: 1px solid #d5d5d5; border-radius: 50%; vertical-align: middle; }
.refund li .info-item-r input { display: none; }
.refund li .info-item-l { color: #3e3e3e; }
.refund li .info-item-l i { color: #40a1f2; font-style: normal; font-size: 12px; }
.refund-qr { display: block;margin: 0 auto 20px; width: 96%; height: 45px; background-color: #ff6102; color: #fff; text-align: center; line-height: 45px; font-size: 16px; }

/*退款*/
.procedure-txt { padding: 13px; font-size: 14px; color: #2f2f2f; }
.procedure-txt p:last-child { color: #797979; margin-top: 3px; }
.procedure { overflow: hidden; margin-top: 12px; padding: 15px 0 8px 8px; background-color: #fff; }
.procedure h2 { padding-bottom: 14px; font-size: 14px; color: #212121; font-weight: normal; border-bottom: 1px solid #ececec; }
.procedure .item { overflow: hidden; padding: 10px 0; border-bottom: 1px solid #ececec; }
.procedure .item:last-child { border-bottom: 0; }
.procedure .item-l { float: left; width: 26px; height: 60px; margin-right: 10px; background: url(../images/procedure.png) no-repeat; background-size: contain; }
.procedure .item-r { float: left; }
.procedure .item-r p:first-child { line-height: 26px; font-size: 13px; color: #555555; }
.procedure .item-r p:last-child { font-size: 11px; color: #939393; }
.procedure .item.old .item-l { background-image: url(data:image/png;base64,); }
.footNav { display: -webkit-box; width: 94%; margin: 0 auto; border-bottom: 1px solid #e6e6e6; }
.footNav a { display: block; -webkit-box-flex: 1; height: 44px; line-height: 44px; text-align: center; font-size: 13px; color: #515151; }
.footNav a:first-child { text-align: left; }
.footNav a:last-child { text-align: right; }
.footLogo { margin-top: 20px; text-align: center; }
.footLogo p { color: #b5b5b5; font-size: 1.083333rem; }
.serviceTip, .procedureTip { display: none; position: fixed; bottom: 0; padding: 10px; overflow: hidden; background-color: #fff; z-index: 1; padding-bottom: 50px; }
.serviceTip h2 { color: #212121; text-align: center; font-weight: normal; font-size: 16px; }
.serviceTip>p { padding-top: 4px; padding-bottom: 12px; text-align: center; color: #939393; font-size: 11px; border-bottom: 1px solid #ececec; }
.serviceTip .item p { padding-left: 25px; position: relative; }
.serviceTip .item p:nth-child(2n+1) { font-size: 13px; color: #424242; height: 30px; line-height: 35px; }
.serviceTip .item p:nth-child(2n) { font-size: 11px; color: #939393; padding-bottom: 16px; border-bottom: 1px solid #ececec; }
.serviceTip .item p:first-child::before { content: ''; position: absolute; left: 0; top: 10px; width: 16px; height: 16px; background-repeat: no-repeat; background-size: contain; }
.serviceTip .item-g::before { background-image: url(data:image/png;base64,); }
.serviceTip .item-b::before { background-image: url(data:image/png;base64,); }
.serviceTip .item-f::before { background-image: url(data:image/png;base64,); }
.serviceTip h3 { font-weight: normal; text-align: center; font-size: 11px; color: #626262; line-height: 26px; margin-top: 5px; }
.service-close { position: absolute; left: 0; bottom: 0; width: 100%; background-color: #ff6102; line-height: 42px; height: 42px; text-align: center; color: #fff; font-size: 12px; }
.procedureTip { width: 96%; padding-right: 3%; padding-left: 3%; margin: 0 auto; overflow: hidden; }
.procedureTip h2 { color: #212121; text-align: center; font-weight: normal; }
.procedureTip .item-r { width: 80%; }
.procedureTip .item-r p { line-height: 18px !important; color: #555555; font-size: 12px !important; }
.procedureTip .item-r p:first-child { margin-bottom: 3px; margin-top: 2px; }
.procedureTip .item:last-of-type { border-bottom: 0; }

.sex{
    position: absolute;
    right: 10px;
    top: 0;
}
.sex label{
    position: relative;
    display: inline-block;
}
.sex label:first-child{
    margin-right: 10px;
}
.sex label input{
    display: none;
}
.sex label::before{
    content: '';
    display: inline-block;
    width: 8px;
    height: 8px;
    border-radius: 50%;
    background-color: #b6b6b6;
    border:5px solid #efefef;
    vertical-align: middle;
}
.sex label.checked::before{
    background-color: #ff6102;
}
.sex label span{
    padding-left: 6px;
    color:#585858;
}

/*遮罩层*/
.Cover { display: none; position: fixed; top: 0; left: 0; width: 100%; height: 100%; background-color: rgba(0, 0, 0, 0.3); }
.animated { -webkit-animation-duration: 1s; animation-duration: 1s; -webkit-animation-fill-mode: both; animation-fill-mode: both }
.slideInUp { -webkit-animation-name: slideInUp; animation-name: slideInUp }
@keyframes slideInUp {
    from { -webkit-transform: translate3d(0, 100%, 0); transform: translate3d(0, 100%, 0); visibility: visible }
    to { -webkit-transform: translate3d(0, 0, 0); transform: translate3d(0, 0, 0) }
}
@-webkit-keyframes slideInUp {
    from { -webkit-transform: translate3d(0, 100%, 0); transform: translate3d(0, 100%, 0); visibility: visible }
    to { -webkit-transform: translate3d(0, 0, 0); transform: translate3d(0, 0, 0) }
}