/*预约成功*/
.booking { margin: 0 auto; padding: 20px 4%; text-align: left; background-color: #fff; }
.booking h2 { height: 30px; line-height: 30px; font-size: 20px; color: #308df6; margin-bottom: 6px; font-weight: normal; }
.booking img { width: 40px; float: left; }
.booking .booking-txt { float: left; width: 82%; margin-left: 12px; margin-bottom: 20px; }
.booking a { display: block; width: 100%; height: 3.166667rem; margin: 0 auto; text-decoration: none; border: 1px solid #e5e5e5; text-align: center; line-height: 3.166667rem; font-size: 16px; color: #5b5b5b; clear: both; border-radius: 3px; }
.booking p { font-size: 13px; color: #484848; }
.booking-ad { padding: 14px 10px; margin-top: 12px; margin-bottom: 24px; background-color: #fff; }
.booking-ad h2 { font-size: 16px; font-weight: normal; margin-bottom: 6px; }
.booking-ad h2::before { content: '惠'; display: inline-block; width: 17px; height: 17px; background-color: #ff9600; font-size: 11px; text-align: center; line-height: 17px; border-radius: 3px; color: #fff; margin-right: 6px; }
.booking-ad p { font-size: 12px; line-height: 20px; color: #7e7e7e; }
.signup, .not-signup { display: block; width: 94%; height: 3.166667rem; margin: 0 auto; border-radius: 3px; font-size: 16px; text-align: center; text-decoration: none; line-height: 3.166667rem; }
.signup { background-color: #308df6; color: #fff; }
.not-signup { background-color: #fff; margin-top: 20px; color: #308df6; border: 1px solid #e5e5e5; }

/*在线报名*/
.course-info { padding: 10px; background: #fff url("data:image/png;base64,") repeat-x bottom; background-size: 12px;}
.course-info p { font-size: 14px; color: #2f2f2f; max-width: 100%; overflow: hidden; text-overflow: ellipsis; white-space: nowrap; }
.course-info p:last-child { margin-top: 6px; line-height: 24px; }

/*优惠金额*/
.money {  margin-top: 10px; border-top: 1px solid #e7e7e7; height: 3.2rem;line-height: 3.2rem;}
.money em{ vertical-align: middle;}
.info-item { overflow: hidden; padding: 0 10px; background-color: #fff; border-bottom: 1px solid #e7e7e7;  font-size: 1.2rem;}
.info-item .info-item-r { float: right; color: #5a5a5a; }
.info-item .info-item-l { float: left; }
.payment { margin-top: 10px; font-size: 1.2rem; }
.payment li { height: 3.2em; line-height: 4rem; }
.payment li:first-child .info-item-r { color: #ff5a00; font-size: 16px; font-weight: bold; }
.payment li:first-child .info-item-r em { font-size: 10px; font-weight: normal; }
.payment li:not(:first-child) .info-item-l::before { content: ''; position: absolute; left: 0; top:50%;margin-top:-11px; display: inline-block; width: 23px; height: 23px; background: url("data:image/png;base64,"); background-size: contain; margin-right: 14px; }
.payment li.ZFB .info-item-l::before { background-image: url("data:image/png;base64,"); }
.payment li.YHK .info-item-l::before { background-image: url("data:image/png;base64,"); }
.payment li.YHK span, .payment li.WeChat span, .payment li.ZFB span { padding-left: 36px; position: relative; }
.payment li input { display: none; }
.payment li:not(:first-child) .info-item-r::before { content: ''; display: inline-block; width: 20px; height: 20px; border: 1px solid #d5d5d5; border-radius: 50%; vertical-align: middle; }
.payment li:not(:first-child) .info-item-r.on::before { border: 0; background: url("data:image/png;base64,") no-repeat; background-size: contain; }
.payment-qr { margin-top: 25px; padding: 0 2%; }
.payment-qr a { display: block; width: 100%; height: 45px; background-color: #ff6102; font-size: 16px; text-align: center; line-height: 45px; color: #fff; text-decoration: none; margin-bottom: 1.1rem; }
.payment-qr span { display: inline-block; position: relative; font-size: 12px; color: #636363; margin-right: 1.5rem; padding-left: 16px; }
.payment-qr span::before, .welfare span::before { content: ''; position: absolute; left: 0; top: 0; display: inline-block; width: 12px; height: 12px; background: url("data:image/png;base64,"); background-size: contain; }
/*优惠信息*/
.Discount p { padding-left: 12px; }
.Discount h3 { position: relative; padding-left: 38px; font-size: 14px; color: #2f2f2f; font-weight: normal; margin-top: 1rem; }
.Discount h3::before { content: '返'; position: absolute; left: 14px; top: 0; width: 17px; height: 17px; background-color: #ff9600; border-radius: 3px; color: #fff; line-height: 17px; text-align: center; font-size: 11px; }
.Discount p { padding-right: 12px; font-size: 12px; color: #7e7e7e; margin-top: 10px; }
.Discount-in { padding-right: 1rem; padding-bottom: 1rem; position: relative; }
