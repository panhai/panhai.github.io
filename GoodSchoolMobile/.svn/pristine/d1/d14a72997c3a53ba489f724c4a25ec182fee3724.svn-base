    <script type="text/javascript">
        $('.schoolTab').find('span').click(function (event) {
            var id = $(this).attr("data");
            var top = $('#' + id).offset().top - 50;
            $("html,body").animate({
                scrollTop: top
            }, 300);
            $(this).siblings('').removeClass('cur');
            $(this).addClass('cur');
        });

        $("#cd_PrdtNav1 .tabMore").click(function () {
            if (!$(this).hasClass("tabStop")) {
                $(".schoolTabMain").addClass("opendiv");
                $(this).addClass("tabStop");
                $(this).find("label").html("收起");
            } else {
                $(".schoolTabMain").removeClass("opendiv");
                $(this).removeClass("tabStop");
                $(this).find("label").html("展开查看全部");
            }            
        })

        //浮动置顶

        var isIE = !!window.ActiveXObject;
        var isIE6 = isIE && !window.XMLHttpRequest;

        $(window).scroll(function () {

            var offset = $('.schoolTab').offset(),
                startTop = offset.top,
                startLeft = offset.left,
                bottomTop = startTop + $('.grey').height(),
                toolHeight = $('.schoolTab').height(),
                scrollTop = $(window).scrollTop();

            if (scrollTop < startTop) {
                $('#fixedNav').hide();
            } else if (scrollTop >= startTop && scrollTop <= bottomTop) {
                if (scrollTop >= bottomTop - toolHeight) {
                    $('#PrdtNav').addClass('PrdtNav_on').css({ top: bottomTop - toolHeight });
                }
                else {
                    $('#fixedNav').show();
                    if (isIE6) {
                        $('#PrdtNav').css({ position: 'absolute', top: scrollTop });
                    }
                }
                
            } else {
                $('#fixedNav').show();
            }

            
        });

    </script>

