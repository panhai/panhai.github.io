	$(".specialty-menu").find(".gr-optionbox02 li .close").show();
	$(".specialty-menu").find(".gr-optionbox02 .change:last").remove();
	$(".specialty-menu").show();
	
	iScrollClcssNav && iScrollClcssNav.refresh();
	iScrollList && iScrollList.refresh();
});






$(".specialty-menu-left p").click(function(){
	$(this).addClass("now").siblings().removeClass("now");
	$(".specialty-menu-right .listmove").eq($(this).index()).show().siblings(".listmove").hide();
});

$(document).on('click','.gr-optionbox02 .close',function(e){
	var _This=$(this).closest("li").attr("title");
	var _ThisDi=$(this).closest(".gr-optionbox02").attr("title");
	
	$(".gr-optionbox02[title="+_ThisDi+"] li[title="+_This+"]").remove();
	
});

	
$(".specialty-menu-right a").click(function(){
	var LiLenght=$(this).parents(".specialty-menu").find(".gr-optionbox02 li").length;
	if(LiLenght>2){
		var Arraycolor=['0',"1",'0',"1"]
		for(var i=0; i<Arraycolor.length; i++){
			$(".specialty-menu .tips").delay(100).animate({"opacity":Arraycolor[i]},1)
		}
	}else{
		var This=$(this).data("id");
		var Vaule=$(this).parents(".specialty-menu").children(".gr-optionbox02").attr("title");
		var Li=$(".gr-optionbox02[title="+Vaule+"] li[title="+This+"]").length;
		
		if(Li>0){
			return false;
		}
		
		$(".gr-optionbox02[title="+Vaule+"]").prepend("<li class='option' title="+This+"><a href='#'><span class='close'>×</span>"+$(this).html()+"</a></li>");
		$(".specialty-menu").find(".gr-optionbox02 li .close").show();
		
		var Ids=$(".specialty-menu .gr-optionbox02 li").map(function(index, element) {
			return $(this).attr("title")
		}).get().join(',');
		
	}
});

$(".gr-search-menu .icon").click(function(){
	$(".gr-search-menu").hide();
	$(".backgroundwhite").hide();
});

$(".gr-optionbox02 .option").click(function(){
	$(this).addClass("now").siblings(".option").removeClass("now");
});





$(".specialty-menu01 .specialty-menu-left p").click(function(){
	var oIndex = $(this).index();
	$(this).addClass("now").siblings().removeClass("now");
	$(".specialty-menu01 .specialty-menu-right .listmove")
		.eq(oIndex).show()
		.siblings().hide();
