/*四步的滑动页*/
.wrapper { width:100%; height:160px; float:left; position:relative;	 z-index:1;	 overflow:hidden;  }
.scroller { width:2100px; height:100%; float:left; padding:0;}
.scroller ul { list-style:none; display:block; float:left; width:100%; height:100%;  text-align:left; }
.scroller li { -webkit-box-sizing:border-box; -moz-box-sizing:border-box; -o-box-sizing:border-box; box-sizing:border-box; display:block; float:left; width:300px; height:160px; text-align:center; }
.scroller li img{ width:213px; height:246px; margin-top:15%; -webkit-box-shadow: 0 0 3px #999;  -moz-box-shadow: 0 0 3px #999;   box-shadow: 0 0 3px #999; }
.scroller li .text{ line-height:20px; color:#999;}
.scroller li .title{ padding:20px 0 10px 0; font-weight:bold;}
.scroller li .c-red{ color:#ed7a58;}
.scroller li .c-blut{ color:#4293dd;}
.scroller li .c-green{ color:#5cb946;}
.scroller li .btn{ height:40px; line-height:40px; width:50%; border-radius:4px; color:#ed7a58; border:1px solid #ed7a58; text-align:center; display:inline-block; margin-top:10px;}
.indicator{ position:fixed; left:50%; bottom:30px; margin-left:-40px;width:80px;}
.indicator, .indicator li { display:block; float:left; list-style:none; }
.indicator li { text-indent:-9999em; width:8px; height:8px; border-radius:8px;  background:#ddd; overflow:hidden; margin-right:10px;}
.indicator li.active { background:#58abf8; }
.indicator li:last-child { margin:0; }



.templateLayer{
    width: 100%;
}
.templateLayer.ofh{
    overflow: hidden;
}
.templateLayer.white{
    background: #ffffff;
}
.templateLayer .titleLayer{
    font-size: 0.74rem;
    color: #4d4d4d;
    padding:.4rem 0 .4rem .63rem ;
    background: rgb(246,246,246);
}
.templateLayer .statusNav{
    background: #ffffff;
    -webkit-display: flex;
    -moz-display: flex;
    -ms-display: flex;
    -o-display: flex;
    -khtml-display: flex;
    display: flex;
    flex-direction: row;
    padding:0.76rem .405rem;
    position: relative;
}
.templateLayer .statusNav .statusBox{
    background: #ffffff;
    -webkit-flex-grow: 1;
    -moz-flex-grow: 1;
    -ms-flex-grow: 1;
    -o-flex-grow: 1;
    -khtml-flex-grow: 1;
    flex-grow: 1;
    display: inline-block;
    width: 3.53rem;
    height: 1.58rem;
    line-height: 1.58rem;
    text-align: center;
    border-radius: 0.05rem;
    border: solid 0.03rem #dedede;
    font-size: 0.68rem;
    color: #a3a3a3;
