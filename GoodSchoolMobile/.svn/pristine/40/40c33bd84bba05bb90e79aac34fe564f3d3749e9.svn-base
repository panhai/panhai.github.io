.bao-title{text-align:center; padding:10px 0; font-size:0.8rem; font-weight:bold;}



/*在同行内的同等子元素*/
.displaybox{display:-moz-box; display:-webkit-box; display:box;}
.displayflex{-moz-box-flex:1; -webkit-box-flex:1; box-flex:1; width:100%; display:block;}


/*学校优惠详情*/
.detail{ font-size:14px; background:white; border:1px solid #d2e1e9; border-radius:4px; position:relative; margin:12px;}
