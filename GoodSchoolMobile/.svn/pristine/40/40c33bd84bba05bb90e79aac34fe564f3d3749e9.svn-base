.school-aera .count .icon1{ position:absolute; top:18px; right:10px; font-size:16px; color:#999;}
.school-aera .count .text{ font-size:12px; margin-top:8px; margin-right:25px; color:#999; display:-webkit-box; overflow:hidden; -webkit-line-clamp:2; -webkit-box-orient:vertical;}

/*查看全部*/
.seeall{ background:#fff; height:38px; line-height:38px; color:#999; font-size:14px;  text-align:center; display:block; }
.seeall span{vertical-align:bottom; height:27px; display:inline-block; color:#b3b3b3;}

