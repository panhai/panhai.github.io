/*优惠券*/
.juan-box{ padding:12px; background:#e8f5ff;}
.juan{ max-width:590px; background:#fff; border:1px solid #dce2e7; margin:15px auto; color:#666;}
.juan .j-content{ height:5rem; border-bottom:2px dotted  #d9d9d9; background:url(../images/juan01.jpg) left top no-repeat; background-size:100% auto;}
.juan .j-content .left{ padding:0.8rem 0 0 0.7rem; float:left; font-size:0.7rem; font-weight:bold; color:#ff6600;}
.juan .j-content .left .big{ font-size:1.8rem;}
.juan .j-content .right{ float:right; padding:1.5rem 1rem 0 0; }
.juan .j-content .right .name{ font-size:0.8rem; font-weight:bold; max-width:7.5rem; overflow:hidden; text-overflow:ellipsis; white-space:nowrap; text-align:center;}
.juan .j-content .right .line{ height:7px; line-height:7px; color:#e5e5e5;}
.juan .j-content .right .text{ font-size:0.6rem; max-width:7.5rem overflow:hidden; text-overflow:ellipsis; white-space:nowrap; text-align:center;}
.juan .see{}
