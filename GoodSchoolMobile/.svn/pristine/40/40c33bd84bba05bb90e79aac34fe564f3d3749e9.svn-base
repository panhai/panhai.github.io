/*撑开父容器*/
.clearfix:after {content: "."; display: block; height: 0; clear: both; visibility: hidden;  }
/*针对IE*/
.clearfix { /*float:none !important; clear:both !important;*/ zoom:1; }


/*头部顶部*/
.Header{ display:block; height:40px; line-height:40px; color:#fff; background:#40a1f2; position:relative; padding-top:1px;/*不可去掉 首页顶栏会出问题*/ }
.Header h1{ padding:0 85px; overflow:hidden; text-overflow:ellipsis; white-space:nowrap; font-size:18px; text-align:center;}
.Header .left{ position:absolute; top:0; left:8px;}
.Header .right{ position:absolute; right:8px; top:0;}
.Header a{ display:inline-block; /*width:40px;*/ line-height:40px; color:#fff; text-align:center; font-size:20px;}
.Header a.icon-arrow-left{ font-size:22px;}


/*课程box*/
.courseinfo{display: -moz-box; display: -webkit-box; display: box; padding: 0.7rem 0; color: #333; background:#fff; border-bottom:1px solid #f2f2f2; margin:0.5rem; border-radius:5px;}
.courseinfo .imgbox{/*width: 80px; height: 80px;*/ width:4.2rem; height:4.2rem; border:1px solid #e3e8ea; margin-left:0.4rem;}
.courseinfo .imgbox img{ /*width:80px; height:80px;*/ width:4.2rem; height:4.2rem;}
.courseinfo .content{ box-flex:1; -webkit-box-flex:1; -moz-box-flex:1; margin:0 0.45rem; font-size:0.7rem; position:relative; }
.courseinfo .content .title{font-size:0.8rem; font-weight:bold; line-height:1.2rem; overflow: hidden; text-overflow: ellipsis; white-space: nowrap; display: block;}
.courseinfo .content .title.row3{ padding-bottom:2px;}
.courseinfo .content .title.row2{ padding-bottom:0.3rem;}
.courseinfo .content .renyi{ font-size:14px; font-weight:bold;}
.courseinfo .content .youhui{ height:0.9rem; line-height:0.9rem; display:inline-block; padding:0 5px; border-radius:4px; border:1px solid #ff4b4e; color:#ff4b4e;}
.courseinfo .content .btn{ width:4.5rem; height:1.4rem; line-height:1.4rem; font-size:0.75rem; text-align:center;  background:#ff3c00; color:white; text-align:center; position:absolute; bottom:0.5rem; right:0; border-radius:5px;}
.courseinfo .content .p{ position:absolute; right:0; bottom:2.3rem; font-size:0.6rem; color:#999;}
.courseinfo .content p{ line-height:1.1rem; font-size:0.6rem; overflow: hidden; text-overflow: ellipsis; white-space: nowrap; display: block;}
.courseinfo-h{ background:#fff; line-height:30px; display:block; padding:0 10px; border-bottom:1px solid #f2f2f2; color:#333;}
.courseinfo-h .title{ max-width:60%; overflow:hidden; text-overflow:ellipsis; white-space:nowrap; float:left;}
.courseinfo-h i{ float:left;}
.courseinfo-h i:before{ content:"\e605"; font-family:"icomoon";}

.content-item{ padding-bottom:1rem;}


/*活动须知*/
.activity{ background:white; padding:0 10px; 10px 10px}
.activity .h{ font-size:0.8rem; text-align:center; padding-bottom:0.8rem; font-weight:bold;}
.activity .p{ font-size:0.6rem; margin-bottom:1rem; color:#666;}

/*好学校保障*/
