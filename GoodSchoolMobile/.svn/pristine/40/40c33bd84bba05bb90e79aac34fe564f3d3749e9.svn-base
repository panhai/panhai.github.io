.juan .see .title .f-r span:before{ font-family: 'icomoon'; content: "\e607";}
.juan .see .title .f-r span.cur:before{ font-family: 'icomoon'; content: "\e604";}
.juan .see .text{ padding:10px; font-size:12px; display:none;}
.juan.two .j-content{ height:5rem; border-bottom:2px dotted  #d9d9d9; background:url(../images/juan02.jpg) left top no-repeat; background-size:100% auto;}
.juan.two .j-content .left{ padding:0.8rem 0 0 0.7rem; float:left; font-size:0.7rem; font-weight:bold; color:#40a1f2;}

