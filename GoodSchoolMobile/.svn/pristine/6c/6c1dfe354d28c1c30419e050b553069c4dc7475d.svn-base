.payment .codeimg{margin:0 auto;margin-top:1.2rem;border-top:1px solid #d8d8d8;padding-top:.25rem;width:80%;}
.payment .codeimg img{display:inline-block;width:3.12rem;}
.payment .codeimg p{display:block;width:100%;color:#727272;font-size:.22rem;margin-top:.2rem;}





