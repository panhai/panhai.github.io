<script>
$(window).load(function(){
	var t=$(".c_i_details1");
	var minh=window.innerHeight*6;//$(window).height();
	var maxh=t.height();
	if(minh<maxh)
	{
		t.height(minh);
		t.find(".pulldown").click(function(){
			if(!t.state || t.state=='close')
			{
				t.animate({height:maxh});
				t.state='open'
			}
			else
			{
				t.animate({height:minh});
				t.state='close'
			}
		});
	}
	else
	{
		t.css("paddingBottom",'0px')
			.find(".pulldown").hide();
	}

});
</script>