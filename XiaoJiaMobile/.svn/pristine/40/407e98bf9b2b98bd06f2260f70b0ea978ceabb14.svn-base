.threeDround{
    position: relative;
    height: 2.6rem;
}
.threeDround a{
    width: 4.2rem;
    height: 2.6rem;
    -webkit-transition: ease .450s;
    -moz-transition: ease .450s;
    -ms-transition: ease .450s;
    -o-transition: ease .450s;
    transition: ease .450s;
    position: absolute;
    top: 0;
    z-index: 100;
}
.threeDround a.mid{
    -webkit-transform: scale(1);
    -moz-transform:    scale(1);
    -ms-transform:     scale(1);
    -o-transform:      scale(1);
    transform:         scale(1);
    z-index: 100;
    -webkit-opacity: 1;
    -moz-opacity: 1;
    -ms-opacity: 1;
    -o-opacity: 1;
    -khtml-opacity: 1;
    opacity: 1;
    left: calc(50% - 2.1rem);
}
.threeDround a.left{
    left: -.4rem;
    -webkit-transform: scale(.8);
    -moz-transform:    scale(.8) ;
    -ms-transform:     scale(.8) ;
    -o-transform:      scale(.8) ;
    transform:         scale(.8);
    z-index: 50;
    -webkit-opacity: .7;
    -moz-opacity: .7;
    -ms-opacity: .7;
    -o-opacity: .7;
    -khtml-opacity: .7;
    opacity: .7;
}
.threeDround a.right{
    left: calc(100% - 4.2rem + .4rem);
    -webkit-transform: scale(.8);
    -moz-transform:    scale(.8);
    -ms-transform:     scale(.8);
    -o-transform:      scale(.8);
    transform:         scale(.8);
    z-index: 50;
    -webkit-opacity: .7;
    -moz-opacity: .7;
    -ms-opacity: .7;
    -o-opacity: .7;
    -khtml-opacity: .7;
    opacity: .7;
}




