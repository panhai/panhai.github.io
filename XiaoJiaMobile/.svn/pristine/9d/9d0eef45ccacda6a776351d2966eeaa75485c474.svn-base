<script>
    var i = 0;
    var interval = setInterval(function () {
        ++i;
        if(i==1){
            $(".threeDround a:nth-child(1)").addClass("right").removeClass("left");
            $(".threeDround a:nth-child(2)").addClass("left").removeClass("mid");
            $(".threeDround a:nth-child(3)").addClass("mid").removeClass("right")
        }
        if(i==2){
            $(".threeDround a:nth-child(1)").addClass("mid").removeClass("right");
            $(".threeDround a:nth-child(2)").addClass("right").removeClass("left");
            $(".threeDround a:nth-child(3)").addClass("left").removeClass("mid")
        }
        if(i==3){
            $(".threeDround a:nth-child(1)").addClass("left").removeClass("mid");
            $(".threeDround a:nth-child(2)").addClass("mid").removeClass("right");
            $(".threeDround a:nth-child(3)").addClass("right").removeClass("left");
            i=0
        }
    },3000)


</script>






