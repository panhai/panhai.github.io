/* 整站通用样式 */
*{ margin:0px; padding:0px;}
ul,ol,li{ list-style:none;}
q{ quotes:none;}
a{ text-decoration:none; cursor:pointer;}
a img{ border:0px;}
.common{ width:1000px; margin:0 auto;}
body{ font-family:"宋体";}
input::-ms-clear{ display:none;}
.clear{ clear:both;}
/*IE6 用absolute模拟fixed*/
body{
	_background-image: url(about:blank);     /*用浏览器空白页面作为背景*/
	_background-attachment: fixed;     /* prevent screen flash in IE6 确保滚动条滚动时，元素不闪动*/
}
/*半透明背景*/
#GlobalBG{ display:none; position:fixed; left:0px; top:0px; width:100%; z-index:55555;}
.black5{ background:rgba(0,0,0,0.5); filter:progid:DXImageTransform.Microsoft.gradient(startColorstr=#4B000000,endColorstr=#4B000000);}
.fixed{ position:fixed; _position:absolute;}
.fixed-top{ top:0px; _top:expression(eval(document.documentElement.scrollTop));}
.top-fixed{
	position:fixed;
	top:0px;
	_position: absolute;
    _top:expression(eval(document.documentElement.scrollTop));
}
.bottom-fixed{
	position:fixed;
	bottom:0px;
	_position: absolute;
	_bottom:auto;
	_top:expression(eval(document.documentElement.scrollTop + document.documentElement.clientHeight - this.clientHeight));
}

/* 页眉 */
.header{ background-color:#fafafa; border-bottom:1px solid #e5e5e5; font-size:12px; text-align:right; height:26px; line-height:26px;}
.header .orange{ color:#ff5002 !important;}
.header,.header a{ color:#818181;}
.header a:hover{ color:#ff5000;}
/* 头部 */
.top{ clear:both; height:118px;}
.top-logo{ float:left; width:234px; position:relative;}
.top-logo em{ position:absolute; top:45%; font-size:24px; color:#666; left:130px; font-style:normal; font-family:"微软雅黑"; white-space:nowrap;}

/*内页尺寸不同*/
.top-inner{ height:98px !important; _position:relative; _z-index:11111;}
.top-inner .top-logo{ height:98px; overflow:hidden;}
.top-inner .top-logo img{ margin-top:-10px;}
/*搜索框*/
.top-search{ float:left; width:586px; height:33px; border:#FF5000 2px solid; margin-top:35px; _background-color:#ff5000; position:relative; z-index:11111;}
.top-search-i{ display:block; float:left;}
.top-search-text{ width:369px; height:33px; border:0px; background-color:#fff; padding:0px 10px; _padding:0px 8px; line-height:33px;}
.top-search-item{ width:66px; height:33px; background-image:url(../images/search-item.jpg); line-height:33px; text-indent:12px; font-size:12px; cursor:pointer; border:0px; border-left:2px solid #ff5000; padding-right:33px;}
.top-search-submit{ width:96px; height:33px; background-image:url(../images/search-submit.jpg); border:0px; cursor:pointer;}
/*地名列表*/
.top-search-list{ position:absolute; width:394px; background:#fff; border:1px solid #2578E2; top:35px; right:-2px; z-index:111; display:none;}
.top-search-list dt{ background:#2578e2; height:34px; line-height:34px; color:#fff; font-size:14px; padding:0px 6px; font-weight:bold;}
.top-search-address b{ display:block; border-bottom:1px solid #c4c4c4; font-size:14px; color:#333; line-height:38px; height:38px; padding:0px 10px;}
.top-search-address div{ padding:10px; *margin-bottom:10px;}
.top-search-address div a{ display:block; width:46px; float:left; font-size:12px; color:#565656; height:15px; margin-bottom:10px; *margin-bottom:0px;}
.top-search-address a:hover{ color:#ff5000;}
/*输入后过滤的地名列表*/
.top-filter-list{ display:none; position:absolute; background:#fff; top:35px; right:-2px; z-index:111; width:225px; border:1px solid #2677E2; padding-bottom:10px;}
.top-filter-list dt{ clear:both; line-height:25px; height:25px; margin-top:5px; font-size:12px; color:#999; border-bottom:1px dashed #BEBEBE; padding:3px 15px; margin-bottom:10px; white-space:nowrap; overflow:hidden; text-overflow:ellipsis;}
.top-filter-list a{ display:block; clear:both; line-height:25px; height:25px; margin-top:5px; color:#323232; font-size:12px; padding:0px 10px;}
.top-filter-list a:hover,.top-filter-list a.cur{ color:#fff !important; background-color:#2577E3 !important;}
/*关键字*/
.top-keywords{ float:left; width:586px; font-size:12px; line-height:34px;}
.top-keywords,.top-keywords a{ padding:0px 3px; color:#4f4f4f;}
.top-keywords a:hover{ color:#ff5000;}
/*二维码*/
.top-code{ width:85px; height:81px; border:1px solid #ececec; float:right; margin-top:20px; position:relative; text-align:center;}
.top-code q{ display:block; width:17px; height:17px; position:absolute; top:-1px; left:-19px; border:1px solid #ececec; font-size:14px; color:#bdbdbd; line-height:19px; text-align:center; cursor:pointer;}
.top-code p{ line-height:20px; font-size:12px; color:#ff5000;}
/* 导航 */
.nav{ clear:both; background-color:#2677E2; height:40px;}
.nav-inner{ *position:relative; *z-index:1111; margin-bottom:20px;}
.nav .common{ position:relative; z-index:1111;}
.nav-head{ width:230px; height:40px; float:left; background-color:#F95C15; color:#fff; text-indent:34px;}
.nav-head samp{ font-size:14px; line-height:40px; font-weight:bold;}
.nav-inner .nav-head samp i{ display:inline-block; width:12px; height:12px; background:url(../images/icons.gif) -36px 0px no-repeat; float:right; overflow:hidden; margin:14px 30px 0px 0px;}
/*副导航*/
.nav-menu{ position:absolute; left:0px; width:226px; height:368px; border:2px solid #ff5000; z-index:11; top:40px; border-top:0px; background:#fff; font-size:0px;}
.nav-menu-inner{ display:none;}
.nav-menu li{ height:37px; background-image:url(../images/index/menubg.jpg); color:#2c2c2c; font-weight:normal; text-indent:0px;}
.nav-menu li p{ font-size:14px; text-indent:14px; line-height:37px;}
.nav-menu li.last{ height:34px;}
.nav-menu li.last p{ _line-height:34px;}
.nav-menu li p.active{ position:absolute; width:225px; height:30px; line-height:32px; color:#ff5000; font-weight:bold; margin:1px 0px 0px 1px; border:2px solid #ff5000; border-right:0px; background-color:#fff; z-index:2222;}
/*子导航*/
.sub-menu{ display:none; position:absolute; left:226px; top:1px; border:2px solid #ff5000; background:#fff; width:710px; z-index:1111; min-height:365px; _height:365px;}
/*.sub-menu-left{ width:500px; float:left;}*/
.sub-menu-left{ clear:both;}
.sub-menu-left dl{ width:/*460px*/96%; float:left; border-bottom:1px solid #e6e6e6; margin:12px 12px 0px 12px; _margin:12px 6px 0px 6px;}
.sub-menu-left dt{ width:120px; float:left; color:#0066CD; text-align:right; font-size:12px; font-weight:bold;}
.sub-menu-left dd{ float:left; width:520px;}
.sub-menu-left a{ font-size:12px; color:#555; display:inline-block; padding-left:10px; margin-left:10px; border-left:1px solid #e6e6e6; margin-bottom:12px;}
.sub-menu-left a:hover{ color:#ff5000;}
.sub-menu-right{ width:195px; float:right; background:url(../images/sub-menu-right.jpg) left top repeat-y; padding-left:10px; height:100%;}
.sub-menu-right q{ position:absolute; right:0px; top:0px; font-size:14px; color:#666; display:block; width:20px; height:20px; line-height:20px; text-align:center;}
.sub-menu-right b{ display:block; line-height:35px; font-size:12px; font-family:"微软雅黑"; color:#666;}
.sub-menu-right .even,.sub-menu-right .odd{ width:88px; height:38px; border:1px solid #efefef; border-width:0px 1px 1px 0px; display:block; float:left;}
.sub-menu-right .odd{ border-left-width:1px;}
.sub-menu-right .first{ border-top-width:1px;}

/*主导航*/
.nav-body{ width:622px; float:left; height:40px;}
.nav-body li{ float:left; margin-left:15px; position:relative;}
.nav-body li i{ display:inline-block; width:14px; height:14px; overflow:hidden; background-image:url(../images/nav-sub-ico.gif); background-position:bottom; position:relative; top:2px; top:0px\9;}
.nav-body li.first{ margin-left:28px; _margin-left:24px;}
.nav-body a{ display:block; line-height:38px; color:#fff; font-size:14px; font-weight:bold; padding:0px 15px; border-top:2px #2677E2 solid;}
.nav-body a:hover,.nav-body .cur{ background-color:#fff; color:#2677E3; border-top-color:#F95C15;}
.nav-body a:hover i,.nav-body .cur i{ background-position:top;}
.nav-foot{ width:147px; height:40px; float:right; background-image:url(../images/nav-foot.jpg); cursor:pointer; position:relative; z-index:1111;}
.nav-foot ul{ position:absolute; top:40px; left:0px; width:147px; background:#fff; z-index:1111; padding:13px 0px; display:none;}
.nav-foot ul li{ line-height:26px; padding-left:13px;}
.nav-foot ul a{ color:#0657C3; font-size:14px;}
/*新增主导航的下拉*/
.nav-sub{ height:0px; position:relative; overflow:visible; z-index:1110; display:none;}
.nav-sub p{ width:100%; text-align:center; top:0px; position:absolute; background:url(../images/nav-subbg.jpg) repeat-x; height:33px; line-height:34px; font-size:14px;}
.nav-sub p a{ padding:0px 20px; color:#333;}
.nav-sub p a:hover{ color:#ff5000;}
.nav-sub-inner{ top:-20px;}

/*页脚*/
.footer{ clear:both; border-top:1px solid #e1e1e1; padding:25px 0px;}
.footer-inner{ background:#f5f5f5;}
/*第一层杂项*/
.tail{ text-align:center; margin-bottom:20px;}
.tail *{ display:inline-block; vertical-align:middle; font-size:12px;}
.tail a,.tail samp{ color:#4a4a4a;}
.tail a:hover{ color:#ff5000;}
/*前两块双行字*/
.tail samp{ text-align:left; margin-right:20px;}
.tail strong{ color:#0055C4;}
/*两个微博链接*/
.tail .icon{ border:1px solid #d9d9d9; *margin-bottom:1px;}
/*尾导航*/
.tail-nav{ *margin-left:15px; _margin-left:0px; font-size:0px;}
.tail-nav a{ display:inline-block; border-right:1px solid #a3a3a3; padding:0px 10px; font-size:12px;}
.tail-nav a:hover{ color:#ff5000;}
.tail-nav a.first{ border-left:1px solid #a3a3a3;}
.footer-inner .tail-nav a{ border-left:1px solid #a3a3a3; border-right:0px;}
.footer-inner .tail-nav a.first{ border:0px;}
/*ie67*/
.tail{ white-space:nowrap;}
.tail-hotline{ *width:86px;}
.tail-time{ *width:120px;}
/*版权*/
.copyright{ text-align:center; line-height:200%; color:#969696; font-size:12px;}
/*浮动框*/
.float{/* position:absolute; top:0px;*/ right:0px; z-index:2;}
.float-link{ display:block; width:40px; height:33px; background-color:#2677E2; color:#fff; font-size:14px; font-weight:bold; text-align:center; padding:5px;}
.float-top{ margin-top:1px; display:block; width:50px; height:50px; background:url(../images/float-top.jpg) top center;}
.float-top:hover{ background-position:bottom center;}
.float-code{ display:block; width:40px; border:1px solid #eaeaea; height:49px;}

/*分页---高级搜索页用*/
.pages{ float:right; margin-top:20px; font-size:12px;}
.pages span,.pages a,.pages b{ display:block; float:left; width:23px; height:23px; line-height:23px; text-align:center; font-size:12px; border:1px solid; margin-right:5px;}
.pages a{ border-color:#ababab; color:#1256A9;}
.pages b{ border-color:#1256A9; background:#27c; color:#fff; font-weight:normal;}
.pages .prev i,.pages .next i{ background-image:url(../images/icons.gif); width:12px; height:12px; display:inline-block; font-size:0px; vertical-align:text-bottom; vertical-align:middle\9;}
.pages a.prev,.pages a.next{ width:auto !important; padding:0px 4px;}
.pages span{ border-color:#ccc;}
.pages a.prev i{ background-position:bottom center;}
.pages a.next i{ background-position:center top;}
.pages span.prev i{ background-position:right center;}
.pages span.next i{ background-position:left center;}
/*IE67兼容*/
.pages span i{ *margin-top:5px; _margin-top:0px;}
.pages a.next,.pages a.prev{ _height:18px; _padding-top:5px;}
/*跳转*/
.pages .number{ width:20px; border:1px solid #ccc; padding:0px 5px; font-size:12px; height:23px; line-height:23px; vertical-align:middle;}
.pages .btn{ width:38px; height:23px; background:url(../images/pages-btnbg.jpg); border:1px solid #aaa; vertical-align:middle;}

/*分页---企业展示页用*/
.pages2{ float:right; margin:20px 0px 50px; font-size:12px;}
.pages2 span,.pages2 a,.pages2 b{ display:block; float:left; width:34px; height:34px; line-height:34px; text-align:center; font-size:12px; border:1px solid #e7e7e7; margin-left:-1px;}
.pages2 a{ color:#585858;}
.pages2 b{ background:#9a9a9a; color:#fff; font-weight:normal;}
.pages2 .prev i,.pages2 .next i{ background-image:url(../images/icons.gif); width:12px; height:12px; display:inline-block; font-size:0px; vertical-align:text-bottom; vertical-align:middle\9;}
.pages2 a.prev,.pages2 a.next{ width:auto !important; padding:0px 15px;}
.pages2 a.prev i{ background-position:bottom left;}
.pages2 a.next i{ background-position:right top;}
.pages2 span.prev i{ background-position:right center;}
.pages2 span.next i{ background-position:left center;}
.pages2 .prev{ margin-left:0px;}
/*IE67兼容*/
.pages2 span i{ *margin-top:12px; _margin-top:0px;}
.pages2 a.next,.pages2 a.prev{ _height:23px; _padding-top:11px;}

/*分页-全蓝*/
.pages4{ clear:both; text-align:right;}
.pages4 a{ display:inline-block; height:30px; line-height:30px; padding:0px 10px; font-size:14px; color:#fff; background-color:#4A97FF;}
.pages4 a:hover{ background-color:#2677E2;}

/*面包屑导航*/
.crumbs{ clear:both; border-bottom:2px solid #2677e2; font-size:12px; color:#767676; line-height:30px;}
.crumbs a{ color:#767676;}
.crumbs a:hover{ color:#ff5000;}

/*表单验证*/
.inputText{ border:solid 1px #A6B6CE; line-height:15px; vertical-align:middle; padding:3px;background:url(../images/common/inputBg1.jpg) repeat-x; }
.errorMsg{ padding:2px; line-height:17px; width:auto; border:solid 1px #fb9f00; background:#FAFBED url(../images/common/inputMsgBg1.jpg) 5px center no-repeat; color:red; padding-left:35px; padding-right:10px; margin-left:6px; }
.errorMsg2{ padding:2px; line-height:17px; width:auto; border:solid 1px #FF0000; background:#ffb9b9 url(../images/common/inputMsgBg1.jpg) 5px center no-repeat; color:red; padding-left:35px; padding-right:10px; margin-left:6px; }
.tipMsg{ padding:2px; line-height:19px; border:solid 0px #FCEDB2; background:url(../images/common/inputMsgBg2.jpg) 5px center no-repeat; color:#999999; padding-left:35px; padding-right:10px; margin-left:5px; }
.successMsg{ padding:2px; line-height:19px; border:solid 0px #FCEDB2; background:url(../images/common/inputMsgSuss.png) 5px center no-repeat; color:#999999; padding-left:35px; padding-right:10px; margin-left:5px; }
.loadingMsg{ padding:2px; line-height:17px; border:solid 1px #FCEDB2; background:#FAFBED url(../images/common/msgLoading.gif) 5px center no-repeat; color:#F58B11; padding-left:35px; padding-right:10px; margin-left:6px; }
.tipMsg a{ text-decoration:underline; color:#999999; }
.tipMsg a:hover{ text-decoration:underline; color:blue; }
#aloading{ position:relative; left:10px; top:8px; *left:-20px;}

/*意见反馈*/
.Suggestion{ width:460px; background:#fff;}
.Suggestion .tip{ font-size:12px; color:#2677e3; line-height:20px; margin:5px 20px 15px;}
.Suggestion img{ vertical-align:text-bottom;}
.Suggestion var,.Suggestion dfn{ display:block; clear:both; font-style:normal; font-size:14px; margin:10px 20px;}
.Suggestion var{ margin-top:10px;}
.Suggestion textarea{ width:99%; border:1px solid #d4d4d4; height:165px;}
.Suggestion dfn p{ height:30px; line-height:30px; color:#666;}
.Suggestion .text{ border:1px solid #d4d4d4; width:170px; height:25px; line-height:25px; font-size:12px;}
/*反馈成功结果*/
.SuggestResult{ text-align:center; display:none;}
.SuggestResult p{ font-size:14px; color:#454545; line-height:40px;}
.ResultOK{ font-size:20px !important; color:#009f1e !important; font-weight:bold;}
.Suggestion .btns{ text-align:right;}
.Suggestion .btn{ width:90px; height:30px; margin:0px 10px 20px 0px; cursor:pointer; font-size:14px; font-family:"微软雅黑"; border:0px;}
.Suggestion .submit,.Suggestion .close{ background-color:#2673ec; color:#fff;}
.Suggestion .reset{ background-color:#dcdcdc; color:#333; margin-right:20px;}

/*新增职位搜索列表*/
.top-search-jobs{ display:none; position:absolute; left:-2px; top:35px; border:1px solid #2673ec; width:370px; padding:10px; background-color:#fff;}
.top-search-jobs a{ display:block; font-size:14px; line-height:30px; padding:0px 10px; color:#666;}
.top-search-jobs a:hover,.top-search-jobs a.cur{ background-color:#2673ec; color:#fff;}