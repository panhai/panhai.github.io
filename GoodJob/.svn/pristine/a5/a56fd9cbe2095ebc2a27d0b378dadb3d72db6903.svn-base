<script type="text/javascript">
    $(function () { adminPageInit({ unresize: "" }); });
    $(".education-c").click(function(event) {
        open.call(this,".education")
    });
    var md ;
    $(".education-xx").on("click",'input',function(){
            md = $(this).closest('.education-xx').siblings('.education-yx').find('div');
            if($(this).prop("checked")){
                if(md.find('a').length < 3 ){
                    md.append($('<a data-id='+$(this).data('id')+'>'+$(this).closest("li").text()+'</a>'))
                    md.delegate("a","click",function(){
                        $(".education-xx").find("[data-id='"+$(this).data("id")+"']").prop("checked",false);
                        $(this).remove();
                    })
                } else{
                  
                alert("最多选择3个")
                return false;
             }
            }else{
                 $(this).prop("checked",false);
                 md.find("[data-id='"+$(this).data("id")+"']").remove()
            }
            
       
               
                 
    })
  
    $(".education-qd").click(function(event) {
        
            save.call(this,".education-c")
        
    });
    $(".education-qx").click(function(event) {
        close.call(this,".education")
    });
    $(".education-bx").click(function(event) {
        $(this).closest('.education-m').find("input").prop("checked",false)
        .end()
        .find(".education-yx div a").remove();
    });
    $(".close").click(function(event) {
       close.call(this,".education")
    });
    function open(oprent){
        $(oprent).hide();
        $(this).siblings(oprent).show();
        reset();
    }
    function save(oprent){
       var str = '';
       var ids = [];
       var yx =  $(this).closest(".education-xx").siblings(".education-yx");
       if(yx.find("div a").length == 0 ){
        $(this).closest(".floatL").find("input[type='hidden']").val("");
         $(this).closest(".education").siblings(oprent).text("请选择");
            close.call(this,".education")
       }else{
        yx.find('div a').each(function(index, el) {
            str+=$(this).text()+',';
            ids.push($(this).data('id'))
        });
        $(this).closest(".education").siblings(oprent).text(str)
        $(this).closest(".floatL").find("input[type='hidden']").val(ids)
        close.call(this,".education")
        }
    }
    function close(oprent){
        $(this).closest($(oprent)).hide();
    }
    function reset(){
        $(".xl").each(function(index, el) {
            var oprent  = ".education"
            var id = $(this).val();
            var yx = $(this).siblings('div').find(oprent),str='';
            yx.find('div a').remove()
             $(this).siblings('div').find(".education-xx").find('input').prop("checked",false);
            if(id.length!=''){
                id = id.split(',');
                for (var i = id.length - 1; i >= 0; i--) {
                    
                    $(this).siblings('div').find('.education-yx div').append($('<a data-id='+id[i]+'>'+yx.find('[data-id='+id[i]+']').closest('li').text()+'</a>'))
                    $(this).siblings('div').find(".education-xx").find('[data-id="'+id[i]+'"]').prop("checked","true")
                    str+=yx.find('[data-id='+id[i]+']').closest('li').text()+',';
                };      
                $(this).closest('.floatL').find('.education-c').text(str)
            }
       
        });
    }
    reset();
</script>


