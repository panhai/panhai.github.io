    <script type="text/javascript">
        $("#form_DropDownListContractState").change(function () {
            //alert($(this).val());
            if (parseInt($(this).val()) > 4) {
                $(this).parent().parent().find(".floatL").hide();
                $(this).parent().show();
            } else {
                $(this).parent().parent().find(".floatL").show();
            }
        });
        $("#form_DropDownListContractState").change();
        function checkAndGotoNextStep(currentStep) {
            if (JBValidatorContainer.checkAllChildren(document.getElementById("addStep" + currentStep + "Container"))) {
                $("#addStep" + currentStep + "Container").hide();
                $("#addStep" + (currentStep + 1) + "Container").show();
                setAddSchoolStep(currentStep + 1);
            }
        }
        function checkAndGotoPrevStep(currentStep) {
            $("#addStep" + currentStep + "Container").hide();
            $("#addStep" + (currentStep - 1) + "Container").show();
            setAddSchoolStep(currentStep - 1);
        }
        function setAddSchoolStep(st) {
            var addSchlStep = ["第一步、填写基本信息", "第二步、填写学校介绍", "第三步、完成"];
            stepTabMenus("stepTabContainer", addSchlStep, st);
        }
        function addFinished() {
            $("#addStep2Container").hide();
            $("#addStep3Container").show();
            setAddSchoolStep(3);
        }
        setAddSchoolStep(1);
    </script>
   
