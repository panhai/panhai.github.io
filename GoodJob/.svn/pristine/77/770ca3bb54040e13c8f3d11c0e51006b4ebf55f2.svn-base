    <script>

        //点击切换Banner
        $(".banner .page p").click(function () {
            $(this).siblings().removeClass("iconBg");
            $(this).addClass("iconBg");
            i=$(this).data("num");
            $(".bannerbox").children().fadeOut(600);
            $(".bannerbox").children(".banner"+i).fadeIn(600);
        });


        //动画显示计算
        var cont = $(".content .preferred");
        function according() {
            cont.map(function (i) {
                if($(this).offset().top < $(window).scrollTop()+ $(window).height()*.75){
                    $(this).removeClass("left right");

                }else{
                    if(i%2==0){
                        $(this).addClass("left");
                    }
                    if(i%2==1){
                        $(this).addClass("right");
                    }
                }
            });

        }



        //页面首次加载调用一次，预防页面刷新停留在该文本没内容显示
        according();

        //好工作&才+发展历程  animation 总体调用
        function animetion() {
            $(window).on("scroll",function (e) {
                e.preventDefault();
                according();
            })
        }
        animetion();


        //导航显示隐藏计算公式
        function formula(m) {
            var docuemntHeight = $(document).height(),
                windowHeight = $(window).height();
            var lastHeight = docuemntHeight - windowHeight ;
            if($(window).scrollTop()<=58){
                $(".nav").css("position","absolute").css("background","transparent").css("boxShadow","box-shadow: 0 0 0 1px rgba(255, 255, 255, .2);");
                $(".head .logo p").css("color","#fff");
                $(".head .navigation a").css("color","#fff");
                $(".head .navigation .cur i").css("background","#fff")

            }else {
                $(".nav").css("position","fixed").css("background","rgba(245,245,245,.95)");
                $(".head .logo p").css("color","#333");
                $(".head .navigation a").css("color","#333");
                $(".head .navigation a.cur").css("color","#ff6464");
                $(".head .navigation .cur i").css("background","#ff6464")

            }

            ;
        }
        //初始化计算
        formula(0);


        //滚动时计算
        $(window).scroll(function (e) {
            e.preventDefault();
            var data  = $(window).scrollTop();
            formula();
        });


    </script>
