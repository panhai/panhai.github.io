<script>
var PPT;
$(function(){
	defaultLng=116.316169,defaultLat=40.005567;
	//直接把坐标写上去
	map.refresh();
	//生成地图查看器的效果
	BIGMAP.refresh();
	panorama.setPosition(new BMap.Point(defaultLng,defaultLat));
	//点击公司图片的查看效果
	$(".Pics li img").each(function(){
		$("<img>").attr({
			src:$(this).data("url"),
			title:this.alt
		}).appendTo($("<li>").appendTo($(".PPT ul")));
		$("<img>").attr({
			src:this.src,
			title:this.alt
		}).appendTo($("<li>").appendTo($(".PPT ol")));
	});
	if(isLow()) TextOverflow('h1',12);
});
$(window).load(function(){
	CHIpros(".PPT,.Pics,.Attribs dt");
	////////////////////////////////////////////////////////////////////////////////////////生成图片查看器的效果
	setTimeout(function(){
		PPT=CHIppt(
			".PPT","ul li",
			{indices:".PPT ol",prev:".PPT sub",next:".PPT sup"},
			{auto:false},
			{
				show:function(oJQli){
					$(".PhotoViewer h6").html(oJQli.find("img").attr("title"));
				}
			}
		);
		$(".PhotoViewer").hide().css('visibility','visible');
		$(".Pics li").click(function(){
			CHIbg.open(function(){
				$(".PhotoViewer").fadeIn();
			});
			PPT.show($(this).index());
		});
		$(".PhotoViewer q").click(function(){
			$(".PhotoViewer").fadeOut(function(){CHIbg.close()});
		});
	},1000);
});
</script>