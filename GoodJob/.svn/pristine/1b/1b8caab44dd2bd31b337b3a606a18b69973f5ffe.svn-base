.ViewMode .Color6{ color:#666666;}
.ViewMode .marTop{ margin-top: 4px;}
.ViewMode .Authentica{
    width: 64px;
    height: 24px;
    display: inline-block;
    margin-left: 10px;
    background: url(../../images/v2/icons/resume/authentica.png) no-repeat;
    vertical-align: middle;
}
.Lists{
    width:830px;
    padding-left:60px
}
.Lists .ViewMode-line{
    position: absolute;
    left: 36px;
    width: 2px;
    height: 100%;
    background-color: #f2f2f2;
}
.Lists .ViewMode-line i{
    position: absolute;
    left: -7px;
    top: 8px;
    width: 16px;
    height: 16px;
    background: url("../../images/school/icons/view-line.png") no-repeat center center;
    _background: url("../../images/school/icons/view-line.gif") no-repeat center center;
}

