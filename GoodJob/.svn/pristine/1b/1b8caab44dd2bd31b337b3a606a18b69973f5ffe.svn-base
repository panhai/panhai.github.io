.Download{ z-index:22; width:100%;  height: 50px;background-color:#95b9e9; text-align:center; box-shadow:0 -1px 1px #ccc;}
.Download-in{width: 980px; margin: 0 auto; text-align: right;}
.Download input{ float:right; padding-left: 26px;width:136px; height:50px; color:#fff; cursor:pointer; font-size:16px; cursor:pointer; border:0;}
.Download .Print{
    background: #1562c8 url("../../images/school/icons/resume/Print.png") no-repeat 20px center;
    _background:  url("../../images/school/icons/resume/Print.gif") no-repeat 20px center;
}
.Download .Export{
    background: #3382eb url("../../images/school/icons/resume/Export.png") no-repeat 20px center;
    _background:url("../../images/school/icons/resume/Export.gif") no-repeat 20px center;
}
.Download .Down{
    background: #3382eb url("../../images/school/icons/resume/Save.png") no-repeat 20px center;
    _background: url("../../images/school/icons/resume/Save.gif") no-repeat 20px center;
}

.Download-show{
    position: absolute;
    right: 50px;
    top: -35px;
}
.Download-show span{
    font-size: 14px;
    color: #1562c8;
    padding-left: 20px;
}
.Download-show .export-m{
    margin-left: 34px;
    background: url("../../images/school/icons/resume/Export-m.png") no-repeat center left;
    _background: url("../../images/school/icons/resume/Export-m.gif") no-repeat center left;
}
.Download-show .print-m{
    background: url("../../images/school/icons/resume/print-m.png") no-repeat center left;
    _background: url("../../images/school/icons/resume/print-m.gif") no-repeat center left;
}
.Download-show .down-m{
    cursor: pointer;
    background: url("../../images/school/icons/resume/Save-m.gif") no-repeat center left;
}


.WarmTip{}
.WarmTip dd.CENTERCONTENT{ padding:40px;}
.WarmTip p{ font-size:18px; color:#525252; line-height:200%;}
.WarmTip em{ font-style:normal; color:#f90;}
.WarmTip div{ margin-top:30px; height:30px;}
.WarmTip div label{ float:left; color:#666; font-size:14px;}
.WarmTip .submit{ width:180px; height:38px; float:right; border:0; cursor:pointer; font-size:16px;}
