/*每项标题*/
h3.Topic{ background:url(../../images/school/icons/topicbg.png) repeat-x; height:40px; clear:both; margin-bottom:20px; line-height:37px; font-weight:normal;}
.Topic b{ display:block; float:left; background:url(../../images/school/icons/topic_new.png) no-repeat;_background:url(../../images/school/icons/topic_new.gif)  no-repeat; width:125px; text-align:center; font-size:18px; color:#1562c8;  font-weight:normal;}
.Topic q{ display:block; float:right; background:15px center no-repeat #fff; font-size:14px; color:#448ceb; padding-left:35px; font-style:normal; cursor:pointer;}
.Topic q.edit{ background-image:url(../../images/school/icons/edit.png); background-image:url(../../images/school/icons/edit.gif);}
.Topic q.plus{ background-image:url(../../images/school/icons/plus.png); background-image:url(../../images/school/icons/plus.gif);}
.Topic q.minus{ background-image:url(../../images/school/icons/minus.png); background-image:url(../../images/school/icons/minus.gif); display:none;}
/*资料上边的标题也就是名字*/
h2.Topic{ font-size:32px; margin:10px auto; font-weight:normal; color:#1562c8; position:relative; text-align:center;}
h2.Topic q{ right:0; top:0; position:absolute;}
/*显示块*/
.Container{ margin-bottom: 10px; border: 1px solid transparent;}
/*预览内容*/
.ViewMode{ position: relative;font-size:14px; color:#a4a4a4; line-height:30px; width:830px;
    margin:0 auto;word-break: break-word; text-overflow: ellipsis;
    padding-bottom: 20px;}
.ViewMode b{ color:#525252; font-weight:normal;}
.ViewMode b.cur{color:#fd6126;}
.ViewMode strong{ color:#333; font-size: 16px;}
