/* 2015-9-25 */


/* 头部 */
body{ background-color:#e8f2ff;}
.Main{ position: relative; background:#fff url("../../images/school/view-bg.png") no-repeat; width:980px; margin:110px auto; padding-bottom:20px;}
.view-bg{width: 980px; height: 8px; background: url("../../images/school/view-bg.png") no-repeat; position:relative;bottom:-28px;}
/* 旧的 */
.Left{ border:1px solid #fff; background-color:#fff;}
.Left{ width:813px; float:left; margin-bottom:20px; position:relative\9; z-index:3\9;}
.Right{ width:223px; float:right; position:relative; z-index:1\9; background:none;}
.fixed{ z-index:2\9;}
.bottom0{ position:absolute; bottom:0;}
input.text,.text-extra,textarea{ font-size:14px; color:#1562c8;}
/*顶操作条*/
.Options{ clear:both; margin:10px 0; font-size:14px; line-height:40px; height:40px; width:815px; color:#999999;}
.Options *{ display:inline-block; vertical-align:middle;}
.Options strong{ font-size:18px; color:#525252; font-weight:normal; margin-right:30px;}
.Options input{ display:none;}
.Options .text{ width:155px; height:40px; line-height:40px; border:1px solid #d3e6ff; background-color:#fff; padding:0 10px; margin-right:5px; box-shadow:0 0 6px rgba(77,89,106,0.12);}
.Options .submit,.Options .reset{ width:48px; height:28px; text-align:center; border:0; cursor:pointer;}
.Options .submit{ background-color:#549af4; color:#fff;}
.Options .reset{ background-color:transparent; color:#525252;}
.Options a{ color:#999999;}
.Options a:hover{ text-decoration:underline; color:#1562c8;}
.Options .import{ float:right;}
/*简历分页*/
.Resume-top{
    position: absolute;
    top: 46px;
    width: 100%;
    height: 50px;
    z-index: 555;
}
.Resume-page{
    
    width: 980px;
    margin: 4px auto 0;
    height: 40px;
}
.Resume-page .page{
    float: left;
    width: 866px;
    height: 40px;
}
.Resume-page .page span{
    position:relative;
    display: block;
    float: left;
    width: 95px;
    border: 1px solid #bad7fd;
    background-color: #fbfbfb;
    margin-left: -1px;
}
.Resume-page .page span a{
    width: 84px;
    padding-left: 10px;
    display: inline-block;
    color: #333333;
    font-size: 14px;
    text-align: left;
    line-height: 38px;
    font-style: normal;
}
.Resume-page .page span.next a{
    color: #1562c8;
    text-align: center;
}
.Resume-page .page span.cur{

    background-color: #1562c8;
}
.Resume-page .page span.cur a{
    color: #fff;
}

.Resume-page .page span i{
    position: absolute;
    height:38px;
    line-height: 38px;
    right: 8px;
    color: #cdcfd1;
    font-size: 18px;
    font-style: normal;
    cursor: pointer;
}
.Resume-page span{
    display: inline-block;
    width: 110px;
    line-height: 38px;
    font-size: 14px;
    text-align: center;
}
.Resume-top.fixed{
    position: fixed;
    top: 0;
    background-color: #b7d9ff;
}
/*头像*/
.Photo{ background:url(../../images/school/resume.png) top center no-repeat; height:140px; padding-top:40px; text-align:center;}
#Photo{ width:134px; height:134px; margin:40px auto 0; cursor:pointer;}
.Profile,.Nothing,.Topic{ width:880px; margin:20px auto 40px; clear:both;}
/*什么也没有的提示*/
.Nothing{ height:20px; line-height:20px; padding:50px 0; font-size:0px; color:#525252; text-align:center; overflow:hidden; border:1px dashed #a4a4a4;}
.Nothing *{ display:inline-block; vertical-align:middle; cursor:pointer;}
.Nothing i{ width:16px; height:16px; background-image:url(../../images/school/icons/plus-gray.png); margin-right:10px;}
.Nothing i{ _overflow:hidden; _background-image:url(../../images/school/icons/plus-gray.gif);}
.Nothing span{ font-size:16px;}
/*基本资料*/
.Profile{ text-align:center; width:714px !important; padding:0 !important;}
.Profile i,.Profile s,.Profile u{ display:inline-block; padding-left:30px; overflow:hidden; background-repeat:no-repeat; background-position:left center; text-decoration:none; font-style:normal; vertical-align:top;}
.Profile i{ background-image:url(../../images/school/icons/mobile-gray.png); _background-image:url(../../images/school/icons/mobile-gray.gif);}
.Profile s{ background-image:url(../../images/school/icons/email-gray.png); _background-image:url(../../images/school/icons/email-gray.gif);}
.Profile u{ background-image:url(../../images/school/icons/idcard-gray.png); _background-image:url(../../images/school/icons/idcard-gray.gif);}
