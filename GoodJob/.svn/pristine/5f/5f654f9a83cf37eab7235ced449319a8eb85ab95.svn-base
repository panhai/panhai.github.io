    <script type="text/javascript">
        function addRoleGroup(elm) {
            editOrAddRoleGroup(elm, "添加新管理员角色", 0, "", 0);
        }
        function editRoleGroup(elm,id,name,v) {
            if(v==1){ alert("不能操作超管"); return;}
            editOrAddRoleGroup(elm, "编辑管理员角色", id, name, v);
        }
        function editOrAddRoleGroup(elm, txt, id, name, v) {
            $("#form_HiddenFieldRolePower").val(v);
            $("#form_HiddenFieldId").val(id);
            $("#addRoleContainer input:text").first().val(name);
            JB.fireEvent($("#addRoleContainer input:text").get(0), "change");
            $("#addRoleContainer input:checkbox").each(function () {
                this.checked = ((parseInt(this.value) & v) > 0);
            });
            JBDialog.show(elm, document.getElementById("addRoleContainer"), { title: txt, padding: 0, modal: true, pos: [{ to: "win", parent:$(".bodyBlock").first().get(0), parkInside: "center" }] });
        }
        function rolesclick(elm, v) {
            var vv = parseInt($("#form_HiddenFieldRolePower").val());
            if(elm.checked) vv= (vv|v);
            else vv = (vv & (~v));
            $("#form_HiddenFieldRolePower").val(vv);
        }
    </script>

