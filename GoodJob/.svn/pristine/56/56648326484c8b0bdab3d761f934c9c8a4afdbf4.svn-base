<script>
$(function(){
	//收藏提示
	$(".tools .tip q").click(function(){
		$(this).parent().hide();
	});
	$(".icons.favor").click(function(){
		if($(this).hasClass("favored"))
		{//已经收藏了的
			$(this).removeClass("favored").addClass("favor")
				.closest(".tools").find("del.tip").show()
				.end().find("ins.tip").hide();
		}
		else
		{
			$(this).removeClass("favor").addClass("favored")
				.closest(".tools").find("ins.tip").show()
				.end().find("del.tip").hide();
		}
	});
});
</script>