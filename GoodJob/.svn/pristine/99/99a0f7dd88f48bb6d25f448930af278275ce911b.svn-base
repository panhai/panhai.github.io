<script>
var PHOTOIMAGE;
function ChangePhoto(src){
	PHOTOIMAGE.attr({
		fill:'url('+ src +')'
	});
}
setTimeout(function(){
	ChangePhoto('../images/v2/adv.jpg');
},3000);
$(function(){
	////////////////////////////////////////////////////////////////////////////////////////////////////////////////点击重命名
	$(".Options .rename").click(function(){
		$(this).hide()
			.parent().find("input").show()
			.end().find("strong").hide();
	}).parent()
	.find(".submit").click(function(){
		var newname=$(this).prev(".text").val();
		$(this).parent().find("strong").text(newname)
	})
	.next(".reset").addBack().click(function(){
		$(this).parent().find("input").hide()
		.end().find(".rename").show()
		.end().find("strong").show();
	});
	//画头像
	var img=$("#Photo img").remove();
	var paper=Raphael("Photo",128,128);
	PHOTOIMAGE=paper.circle(64,64,60).attr({
		stroke:'#d3e6ff',
		'stroke-width':2,
		fill:'url('+ img.attr('src') +')'
	});
	
	/////////////////////////////////////////////////////////////////////////////////////////////////////////右侧卡住，及滚动监听
	//CHIfixed(".RightNav","top",{backgroundColor:'#fff',border:'1px solid #fff'});
	var ANCHOR_ACTIVE=function(){
		$(this).addClass("cur")
			.closest(".RightNav").find("a").not(this).removeClass("cur");
	}
	$(".RightNav a").click(function(){ANCHOR_ACTIVE.call(this)});
	var ANCHORS=$(".RightNav a").map(function(){
		var href=this.href;
		return href.substr(href.indexOf('#')+1);
	}).get();
	ANCHORS.timer=0;
	function NAV_LISTEN()
	{
		var scrollTop=$(window).scrollTop();
		for(var n=0;n<ANCHORS.length;n++)
		{//要激活的位置，需要在可视区之上或相等，其下位要在可视区之下或之内
			var topicOffset=$("#"+ANCHORS[n]).offset().top;
			if(scrollTop<topicOffset) break;
		}
		ANCHOR_ACTIVE.call($(".RightNav a").get(n? n-1 : 0));
	}
