		container.find(".ViewMode").hide()//隐藏内容预览区（只有有内容时才显示“编辑”）
			.end()
			.find(".EditMode").show()//显示编辑区
		;
	});
	//点击编辑区的确定
	$(".EditMode .submit").click(function(){
		//将所填充的内容收集成数据对象
		$(this).closest(".EditMode").hide();
		var container=$(this).closest(".Container"),
			mode=container.data("mode"),//数据类型，如果为LIST则需要判断添加新的还是修改旧的内容预览区项目
			viewmode=container.find(".ViewMode"),
			topic=container.find(".Topic"),
			bool=true//如果是新的数据则为true，否则为false
		;
		topic.find(".minus").hide();
		if(mode=='SINGLE') topic.find(".edit").show();
		else topic.find(".plus").show();
		viewmode.show();
		if(bool)//如果是新的内容且是列表内容，则要添加一个新的列表项
		{
			var html='';//生成新列表项的HTML字符串
			viewmode.prepend(html);
		}
		//如有需要，就在每个submit上单独绑定额外的事件
		ALERT('提示','操作成功',3000);
	});
	//点击列表项目上的编辑，只有列表项内容才有，且有编辑时无Nothing
	$(".Container").delegate(".Lists dd a:first-child",'click',function(){
	//$(".Lists dd a:first-child").attr('href','javascript:void(0)').click(function(){
		this.href='javascript:void(0)';
		var subid=$(this).data("id"),
			container=$(this).closest(".Container"),
			editmode=container.find(".EditMode");
		//点了一个编辑之后，再点另一个编辑，要先将当前的编辑项目取消
		editmode.find(".reset").click();
		//将要编辑的内容的数据填充到表单中
		FormInit(container.attr('id'),subid);
		$(this).closest(".Lists").before(editmode).hide();
		editmode.show();
	});
	//点击列表项目上的删除，只有列表项内容才有
	$(".Container").delegate(".Lists dd a:last-child",'click',function(){
	//$(".Lists dd a:last-child").attr('href','javascript:void(0)').click(function(){
		this.href='javascript:void(0)';
		var id=$(this).data("id");
		if(confirm('真的要删除本条记录吗？删除之后不可恢复！'))
		{
			//删除数据的过程
			var Anchor=$(this).closest(".Container").attr("id");
			$(this).closest(".Lists").remove();
			delete(Datas[Anchor][id]);
		}
	});
});

