$(function(){
	//点击Nothing里边的添加
	$(".Nothing").click(function(){
		$(this).hide()
			.closest(".Container")
				.find(".EditMode").show()//显示编辑区
				.end()
				.find(".Topic .plus").hide()//显示所属标题上的"取消"，隐藏"添加"
				.siblings(".minus").show()
		;
	});
	//点击标题上的添加
	$(".Topic .plus").click(function(){
		var container=$(this).closest(".Container"),
			viewmodes=container.find(".ViewMode");
		FormReset(container.attr("id"));
		$(this).hide()
			.siblings(".minus").show()//显示“取消”
		container
			.find(".EditMode").show()//显示最近的一个编辑区
			.insertBefore(viewmodes.eq(0));//插入到第一个内容预览之前
		viewmodes.show();//显示列表被隐藏项
		container.find(".Nothing").hide()//隐藏Nothing
		;
	});
	//点击标题上的取消及编辑区的取消
	$(".Topic .minus,.EditMode .reset").click(function(){
		var THIS=this;
		CONFIRM('放弃编辑','<h6>真的要放弃本次的编辑？取消后内容无法恢复！</h6>','确认放弃',function(){
			CENTERDIV.close.call(CONFIRM.JQ.get(0),true);
			var bool=true,//如果有内容则为true，显示内容预览区，否则显示Nothing
				container=$(THIS).closest(".Container"),
				mode=container.data("mode"),//数据类型，SINGLE则显示编辑，LIST则显示添加
				topic=container.find(".Topic")
			;
			topic.find(".minus").hide();
			container.find(".EditMode").hide();
			if(bool)
			{
				if(mode=='SINGLE') topic.find(".edit").show();
				else topic.find(".plus").show();
				container.find(".ViewMode").show();
			}
			else
			{
				topic.find(".plus").show();
				container.find(".Nothing").show();
			}
		});
	});
	//点击标题上的编辑，只有单项内容才有，且有编辑时无Nothing
	$(".Topic .edit,h2.Profile .edit").click(function(){
