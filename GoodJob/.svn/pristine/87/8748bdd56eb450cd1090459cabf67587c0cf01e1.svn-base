<script>
$(function(){
	$(".Infors div").each(function(){
		var paper=Raphael(this,110,110),
			color=$(this).data("color"),
			circle=paper.circle(55,55,50).attr({
				stroke:color,
				'stroke-width':5,
				fill:'#fff'
			}),
			anchor=$(this).closest("a");
		anchor.data({
			circle:circle,
			color:color
		}).css({color:color})
		.mouseenter(function(){
			var color=$(this).data("color"),
				circle=$(this).data("circle");
			circle.animate({
				fill:color,
				stroke:'#eee'
			},300);
		}).mouseleave(function(){
			var color=$(this).data("color"),
				circle=$(this).data("circle");
			circle.animate({
				fill:'#fff',
				stroke:color
			},300);
		});
	});
});
$(window).load(function(){
	CHIpros(".Profile .logo");
});
</script>