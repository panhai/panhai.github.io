/* 名企首页 */
/*幻灯片*/
.Banner{ clear:both; width:100%; height:390px; overflow:hidden;}
.Banner a{ display:block; width:100%; height:100%; background-position:center; background-repeat:no-repeat;}
/*过滤搜索条*/
.Filters{ height:35px; padding:10px; box-shadow:0 5px 5px rgba(117,149,192,0.2); background-color:#fff; position:relative; z-index:2;}
/*.Filters .SELECT{ width:200px; float:left; margin-right:10px;}
.Filters .SELECT dt{ height:35px; line-height:35px; background-color:#f2f8ff;}
.Filters .SELECT dt span,.Filters .SELECT dt b{ width:160px; font-size:16px;}
.Filters .CitiesPicker dt input{ font-size:16px; height:34px; line-height:34px; float:left;}
.Filters .submit{ width:65px; background-color:#1562c8; font-size:16px; color:#fff; border:0; cursor:pointer; float:right; height:35px;}
.Filters .text{ width:320px; font-size:16px; color:#1562c8; background-color:#f2f8ff; float:right; height:35px; line-height:35px; border:0; padding:0 10px;}*/
.Filters *{ font-weight:normal; line-height:35px;}
.Filters h1{ font-size:24px; margin-right:30px; color:#1562c8; float:left; padding-left:55px; background:url(../../images/v2/icons/tower.jpg) 15px center no-repeat;}
.Filters h2{ font-size:14px; padding-top:5px;}
.Filters h2 a{ color:#434343; display:block; margin-right:10px; float:left; width:90px; text-align:center;}
.Filters h2 a:hover{ color:#1562c8; background:url(../../images/v2/icons/schoolbg-hover.png) center no-repeat;}
.Filters h2 a:hover{ _background-image:url(../../images/v2/icons/schoolbg-hover.gif);}
.Filters h2 a.cur{ color:#fff; background:url(../../images/v2/icons/schoolbg-cur.png) center no-repeat;}
.Filters h2 a.cur{ _background-image:url(../../images/v2/icons/schoolbg-cur.gif);}

/*主体*/
.Main{ background-color:#f2f2f2; padding:30px 0;}
.Main li{ float:left; width:25%; text-align:center; font-size:14px; line-height:30px;}
.Main li div{ display:block; width:180px; height:380px; background-color:#fff; margin:0 auto; padding:20px; margin-bottom:50px;}
.Main li var{ display:block; width:140px; height:140px; margin:10px auto;}
.Main li h3{ width:100%; font-weight:normal; white-space:nowrap; overflow:hidden; text-overflow:ellipsis; margin-top:10px; font-size:18px; line-height:50px;}
.Main li p{ width:145px; height:30px; margin: 10px auto;}

/*高等学校*/
.High li div{ border:1px solid #c8e0ff;}
.High li var{ border:2px solid #e3efff;}
.High li h3{ color:#474747;}
.High p{ color:#1562c8; background-image:url(../../images/v2/icons/prof-bg-blue.png); _background-image:url(../../images/v2/icons/prof-bg-blue.gif);}
/*中职学校*/
.Middle li div{ border:1px solid #eeeeee;}
.Middle li var{ border:2px solid #eeeeee;}
.Middle li h3{ color:#1562c8;}
.Middle p{ color:#3a3a3a; background-image:url(../../images/v2/icons/prof-bg-gray.png); _background-image:url(../../images/v2/icons/prof-bg-gray.gif);}
/*高等学校*/
.Skill li div{ border:1px solid #fed219;}
.Skill li var{ border:2px solid #fed219;}
.Skill li h3{ color:#1562c8;}
.Skill p{ color:#3a3a3a; background-image:url(../../images/v2/icons/prof-bg-gray.png); _background-image:url(../../images/v2/icons/prof-bg-gray.gif);}


.PAGES{ padding-right:26px; text-align:right;}
/*很大的更多*/
div.more{ text-align:right; margin-top:15px;}
div.more a{ display:inline-block; height:40px; line-height:40px; color:#fff; padding:0 30px 0 15px; background:url(../../images/v2/icons/chevrons-right.png) 120px center no-repeat #87b8f8; margin-right:22px;}
div.more a{ _background-image:url(../../images/v2/icons/chevrons-right.gif)}

/*新增提示*/
.WarmTip{ width:577px; height:305px; background-image:url(../../images/v2/warmtip-schools.png); _background-image:url(../../images/v2/warmtip-schools.gif); margin:0 auto; position:relative;}
.WarmTip q{ display:block; position:absolute; right:0; top:0; width:55px; height:55px; background-image:url(../../images/v2/icons/times-circle.png); _background-image:url(../../images/v2/icons/times-circle.gif); cursor:pointer;}
.WarmTip a{ display:block; position:absolute; left:320px; top:208px; font-size:18px; color:#1562c8; cursor:pointer;}
.hidden{ display:none;}
