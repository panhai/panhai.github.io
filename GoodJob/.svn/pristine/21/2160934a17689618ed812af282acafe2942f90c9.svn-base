/* 职位详情页 */
body{  background: #f8fbff;}
.Main{ margin-top:35px; margin-bottom:20px;}
.Left{ width:740px; float:left;background:#fff;}
.Right{ width:300px; float:right;}
.HEAD{ background: #fff;}
.NAV{ background: #fff;}
/*顶部详细信息*/
.Attribs{ width:640px; float:left; padding:30px 40px; background-color:#ffffff; margin-bottom: 4px;}
.Attribs dt{ width:148px; height:148px; float:left; margin-right:20px; overflow:hidden; border:1px solid #f0f0f0;}
.Attribs h1{ margin-top: -10px;font-weight: normal;  margin-bottom: 12px; font-size :24px; color:#333; line-height:40px; max-width: 465px; overflow:hidden;text-overflow:ellipsis;white-space: nowrap;}
.Attribs h1 i{ display:inline-block; vertical-align:text-top; width:20px; background:url(../../images/v2/icons/check-s.png) left center no-repeat; height:30px; margin-left:10px;}
.Attribs h1 i{ _background-image:url(../../images/v2/icons/check-s.gif);}
.Attribs h2{ font-size:18px; color:#494949; font-weight:normal; line-height:30px; margin-bottom:10px;}
.Attribs dd{ width:466px; float:left;}
.WELFARE{ float:left; overflow: hidden;}
.WELFARE span{ float: left;margin-bottom:10px;  margin-right: 10px;  width: 100px;  height: 28px;  background: #f0f6ff;  color: #999;  font-size: 14px;  line-height: 28px;  text-align: center;}
.WELFARE u,.WELFARE s{ color:#5593e6; background-image:url(../../images/v2/tag2.png); _background-image:url(../../images/v2/tag2.gif);}
.WELFARE-MORE{  float: right;display: inline-block;  width: 14px;  height: 14px;  background: url(../../images/v2/icons/chevrons-down.png) no-repeat;  cursor: pointer;}
/*子导航*/
.SubNav{ width: 660px; margin: 0 auto;background:#fff; clear:both; height:45px; border-bottom:2px solid #438ff2; line-height:45px; font-size:18px;}
.SubNav a{ display:block; float:left; width:120px; text-align:center; color:#1562c8;}
.SubNav a:hover,.SubNav a.cur{ background-color:#5998ed; color:#fff;}

/*三块描述通用样式*/
.Intros{ clear:both; padding:0px 35px; background-color:#ffffff;}
.Intros dt{ clear:both; color:#1562c8; padding:26px 0 20px; }
.Intros dt h3{ font-size:18px; height:20px; line-height:20px; font-weight: normal;}
.Intros dd{ clear:both; font-size:14px; line-height:32px; color:#666666; top:38px;}
/*公司介绍*/
.Introduction{ overflow:hidden; padding-bottom:20px;}
.Introduction p{}
.Introduction h4{ font-weight:normal; font-size:18px;}
/*公司图片*/
.Pics{ width:102%; float:left;}
.Pics li{ width:158px; height:118px; float:left; border:1px solid #d1e5ff; margin:0 8px 10px 0; overflow:hidden; cursor:pointer;}
.Address{    padding: 0;  background-color: #ffffff;}
.Address h2 {  border-bottom: 1px solid #f2f2f2;  font-size: 15px;  color: #333333;  padding-left: 26px;  line-height: 48px;}
.Address p {  font-size: 14px;  color: #666666;  padding: 6px 26px 12px;  line-height: 30px;}
.MAPTHUMB {  padding: 0 26px;  }
/*搜索栏*/
.Job-Search{ height:42px; margin-bottom:20px;}
.Job-Search .SELECT{ width:125px; float:left; margin-right:10px;}
.Job-Search .SELECT dt{ border:1px solid #d5e7ff; line-height:38px; padding:0 10px;}
.Job-Search .SELECT dt span,.Job-Search .SELECT dt b{ width:85px;}
.Job-Search .submit{ float:right; width:110px; height:40px; background-color:#70a7f1; font-size:18px; cursor:pointer; border:0; color:#fff;}
.Job-Search .text{ float:right; width:260px; height:38px; line-height:38px; border:1px solid #d5e7ff; padding:0px 10px; font-size:14px; color:#1562c8;}
/*新增部门*/
.Branches{}
.normal{ font-size:14px; border:1px solid #d5e7ff; width:122px;}
/*职位列表*/
.Jobs{}
.Jobs li{ clear:both; height:76px; padding-bottom: 10px;margin-bottom: 10px; background-color:#fff; line-height:30px; border-bottom: 1px solid #f2f2f2;}
.Jobs h4{ font-weight:normal; float:left; padding-top:10px;}
.Jobs h4 a{ font-size:18px; color:#525252; margin-right:1em; display:inline-block; max-width:20em; white-space:nowrap; overflow:hidden; text-overflow:ellipsis; vertical-align:middle;}
.Jobs h4 a:hover{ color:#1562c8; text-decoration:underline;}
.Jobs h4 span{ color:#a4a4a4; font-size:14px;}
.Jobs samp{ color:#a4a4a4; font-size:16px; float:right; padding-top:5px;}
.Jobs p{ clear:both; font-size:14px; color:#666666;}
.Jobs p span{ color:#f7831c;}
.Jobs .Jobs-l{ float: left;}
.Jobs .Jobs-qt{ float: right;  margin: 23px 12px 0 0;  width: 96px;  height: 28px;  text-align: center;  font-size: 14px;  color: #333333;  border-radius: 3px;  background: #fafafa;  border: 1px solid #d0d0d0;}
/*分页*/
.PAGES{ text-align:right; padding:14px 0 60px 0; padding-top:5px\9;}


/*右侧信息*/
.Company{ background-color:#f8fbff; color:#5593e6; font-size:14px; height:120px; margin-bottom:15px; overflow:hidden;}
.Company dt{ float:left; margin:10px; border:1px solid #86b1e9; width:98px; height:98px; overflow:hidden;}
.Company dd{ margin:25px 10px 0 5px; line-height:22px;}
.Company dd a{ color:#5593e6;}
.Company dd a:hover{ color:#f16702; text-decoration:underline;}
.Company dd i{ display:inline-block; width:20px; height:23px; background-image:url(../../images/v2/icons/check-s.png); _background-image:url(../../images/v2/icons/check-s.gif);}
.Props{ background-color:#ffffff ; color:#656565; line-height:30px; margin-bottom:15px; padding: 16px 26px; font-size: 14px;}
.Props p{ line-height: 30px; word-break:break-all;}
.Props a{ color:#6c6c6c;}
.Props a:hover{ color:#1562c8;}
.Props p span {  color: #999999; }
/*图片查看器*/
.PhotoViewer{ visibility:hidden;}
.PhotoViewer h5{ color:#7a7a7a; font-size:24px; margin-bottom:25px;}
.PhotoViewer q{ position:absolute; right:10px; top:10px; cursor:pointer;}
.PPT{ margin:0 auto;}
.PPT,.PPT ul,.PPT ul li{ width:880px; height:580px; background-color:#ccc;}
.PPT,.PPT ul{ position:relative;}
.PPT ul li{ left:0px; top:0px; position:absolute;}
.PPT ol{ left:4px; bottom:5px; position:absolute; height:83px;}
.PPT ol li{ float:left; width:103px; height:78px; background-color:#adb9c9; overflow:hidden; margin:3px; cursor:pointer;}
.PPT ol li.cur{ border:3px solid #fff; margin:0;}
.PPT sub,.PPT sup{ display:block; position:absolute; top:213px; width:38px; height:72px; cursor:pointer;}
.PPT sub{ left:24px; background-image:url(../../images/v2/icons/chevron-left-huge.png); _background-image:url(../../images/v2/icons/chevron-left-huge.gif);}
.PPT sup{ right:24px; background-image:url(../../images/v2/icons/chevron-right-huge.png); _background-image:url(../../images/v2/icons/chevron-right-huge.gif);}
.PhotoViewer h6{ font-size:18px; color:#fff; margin-top:15px;}
