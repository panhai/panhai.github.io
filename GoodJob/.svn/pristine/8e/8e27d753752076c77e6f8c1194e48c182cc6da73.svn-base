<script>
$(function(){
	//点击暂停
	$(".options a:first-child").attr('href','javascript:void(0)').click(function(){
		var id=$(this).data("id"),THIS=this;
		CONFIRM('取消发布','<h5>您确定要取消发布该职位吗？</h5><p>取消发布后，该职位将移动到"已暂停职位"列表。</p>','确定取消',function(){
			alert("取消过程");
			ALERT('取消发布','<h5>取消成功！</h5>',2000);
			$(THIS).closest("li").remove();
		});
	});
	//如何提高……点叉
	$(".How q").click(function(){
		CENTERDIV.close.call($(".How").get(0),true);
	});
	//新增图标效果
	$(".job h4 ins").hover(function(){
		$(this).addClass("on");
	},function(){
		$(this).removeClass("on");
	});
});
$(window).load(function(){
});
</script>