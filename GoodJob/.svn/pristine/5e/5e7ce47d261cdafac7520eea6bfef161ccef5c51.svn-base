<script>
$(function(){
	//点击查看邀请，将邀请内容及联系方式放入弹出层
	$(".Schools2 q a").click(function(){
		var id=$(this).data('id');
		//Ajax的success方法
		function setContent(data){
			var tel,email,content,name,author;
			$(".Invitation")
				.find("strong[data-attr='name']").text(name)
				.end().find(".disabled").html(content)
				.end().find("span.tel").text(tel)
				.siblings("span.email").text(email)
				.siblings("span.author").text(author)
			;
		}
	});
});
