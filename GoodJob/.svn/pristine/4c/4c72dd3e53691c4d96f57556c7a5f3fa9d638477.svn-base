	color: #fff;
	line-height: 38px;
}

/* 分页 */

.page{
    width: 760px;
    height: 25px;
    margin-bottom: 54px;
    text-align: right;
    float: right;
}
.page a.cur{
    color: #fff;
    background-color: #0b89f4;
}
.page a{
    display: inline-block;
    width: 22px;
    height: 22px;
    margin-left: 5px;
    font-size: 12px;
    text-align: center;
    line-height: 22px;
    color: #0b89f4;
    border: 1px solid #0b89f4;
}
.page .prev, .page .next{
    width: 54px;
   
}

.List .page{
    margin-top: 20px;
}
/*取消申请加入联盟弹出框hover效果*/

.Application .submit:hover{
	background: #EB3900;

}
.CENTERDIV .reset:hover{
	background-color: #B2B2B2;
}