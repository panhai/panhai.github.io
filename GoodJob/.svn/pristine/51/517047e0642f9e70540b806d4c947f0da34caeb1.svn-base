<script>
$(function(){
	//画头像
	var img=$("#Photo img").remove();
	var paper=Raphael("Photo",140,140);
	paper.circle(67,67,67).attr({
		stroke:'#d3e6ff',
		fill:'url('+ img.attr('src') +')'
	});
	
	//移动简历的分类
	Boxes(".Move .groups");
	$(".Move .create").click(function(){
		$(this).hide().next().show();
	});
	$(".Move .sub-submit").click(function(){
		var name=$(this).prev().val();
		function newName()
		{
			//把Ajax出来的信息放进来
			var id='',name='';
			$(".Move .groups").append('<label><input type="checkbox" name="groups" value="'+ id +'">'+ name +'</label>')
				.find("label:last").click();
		}
	});
	$(".Move .sub-reset").click(function(){
		$(".Move .text").val('');
	});
	$(".Move .btn").click(function(){
		$(this).parent().hide().prev().show();
	});
})
</script>
