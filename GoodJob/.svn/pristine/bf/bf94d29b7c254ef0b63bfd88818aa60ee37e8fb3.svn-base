<script>
$(function(){
	LeftMenu();
	TopSelect();
	CenterDiv();
	ConfirmAsk("td q a:nth-of-type(2)",".Agree");
	ConfirmAsk("td q a:last-child",".Refuse");
	AllSelect();
	var Invitation=$(".Invitation")
	$(".options .check").attr('href','javascript:void(0)').click(function(){
		var id=$(this).data('id');
		Invitation.find(".id").val(id);
		fill_show();
		function fill_show(data){//ajax的success
			data={
				company:'公司名称',
				content:'内容',
				school:'学校名称',
				date:'日期',
				author:'联系人',
				tel:'电话',
				email:'邮箱'
			}
			for(var n in data)
			{
				Invitation.find(n).html(data[n]);
			}
			alphabg.open(function(){
				Invitation.fadeIn();
			});
		}
	});
	Invitation.find(".CenterBtns :button").unbind().click(function(){
		var id=Invitation.find("input.id").val();
		if($(this).hasClass("refuse"))
		{
			//拒绝过程
			alert(false);
		}
		else if($(this).hasClass("agree"))
		{
			//同意过程
			alert(true);
		}
		Invitation.fadeOut(function(){
			alphabg.close();
		});
	});
});
</script>