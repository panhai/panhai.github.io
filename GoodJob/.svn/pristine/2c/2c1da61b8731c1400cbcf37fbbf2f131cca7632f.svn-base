	$(".options p:last-child a").click(function(){
		var id=$(this).data("id"),name=$(this).data("name");
		CONFIRM('删除简历','<h6>您确定要删除 # '+ name +' # 的简历吗？</h6>','确定删除',function(){
			alert('删除过程');
		});
	});
	//点击全部删除
	$(".DeleteAll").attr('href','javascript:void(0)').click(function(){
		var boxes=$(":checked");
		if(!boxes.length)
		{
			ALERT('错误提醒',"<h5>未选中任何简历！</h5>");
			return false;
		}
		var ids=boxes.map(function(){return this.value;}).get().join('');
		CONFIRM('批量删除简历','<h6>您确定要删除这些简历吗？<br>您总共选中了'+ boxes.length +'封简历。</h6>','确定批量删除',function(){
			alert('批量删除过程');
		});
	});
	//点击收藏
	$(".options p:first-child a").click(function(){
		var id=$(this).data("id"),name=$(this).data("name");
		//添加过程
		ALERT('添加到HR收藏','<h6>成功将 # '+ name +' # 加入到收藏夹</h6>',2000);
	});
	//////////////////////////////////////////////////////////////////////////////////////////////////////////////填充职位大列表
	var strTH=[],strTD=[],array=positionArray,
		level1Reset=true,level2Reset=true;
	for(var n=0;n<array.length;n++)
	{
		var id=array[n][2],name=array[n][0];
		if(id.indexOf('0000')>=0)
		{//一级名称，加入表头
			if(!level1Reset) strTD.push('</ul>');
			strTH.push('<h3 class="menu">'+ name.replace(/[\|\/]/g,'　') +'</h3>');
			strTD.push('<ul class="FilterSubmenu">');
			level1Reset=false;
			level2Reset=true;
		}
		else if(id.indexOf('00')>=0)
		{//二级名称，加入列表项（需要先结束上个列表项）
			if(!level2Reset) strTD.push('</p></li>');
			strTD.push('<li><h4>'+ name +'</h4><p>');
			level2Reset=false;
		}
		else
		{//三级名称，直接加入链接
			strTD.push('<a data-id="'+ id +'">'+ name +'</a> ');
		}
	}
	strTD.push('</p></li></ul>');
	$(".filter-jobs table").find("th").html(strTH.join(''))
		.end().find('td').html(strTD.join(''));
	//职位选择
	CHItab(".filter-jobs .FilterMenu",".menu",".FilterSubmenu");
	///////////////////////////////////////////////////////////////////////////////////////////////////////////填充行业大列表
	var str=[];
	array=subjectArray;strTH=null;strTD=null;
	level1Reset=true;level2Reset=true;
	for(var n=0;n<array.length;n++)
	{
		var id=array[n][2],name=array[n][0];
		if(id.indexOf('00')>=0)
		{//一级名称，加入表头，并重起一行
			if(!level1Reset) str.push('</td></tr>');
			str.push('<tr><th>'+ name.replace(/[\|\/]/g,'　') +'</th><td>');
			level1Reset=false;
		}
		else
		{//二级名称，直接加入链接
			str.push('<a data-id="'+ id +'">'+ name +'</a> ');
		}
	}
	$(".filter-profs tbody").html(str.join(''));
});
</script>