.Skills p{ font-size:14px; line-height:25px; padding:10px 0;}
.Skills p a{ color:#a4a4a4; margin-right:15px; display:inline-block; white-space:nowrap;}
.Skills p a:hover{ color:#1562c8; text-decoration:underline;}
/*中职/技校*/
.Other{ clear:both; line-height:30px; padding:10px 20px;}
.Other h5{ float:left; font-size:16px; font-weight:normal; margin-right:20px;}
.Other a{ color:#666;}
.Other a:hover{ color:#1562c8; text-decoration:underline;}



