/* 校企合作 */
.Right{ background:none;}
.Query{ padding:10px;}
/*模拟复选框*/
.Boxes{ float:left; font-size:16px; line-height:40px;}
.Boxes a{ display:block; float:left; padding:0 20px; font-style:normal; background:url(../../../images/v2/icons/box-empty.jpg) left center no-repeat; cursor:pointer; color:#525252;}
.Boxes a:hover{ color:#1562c8; text-decoration:underline;}
.Boxes a.cur{ background-image:url(../../../images/v2/icons/box-checked.jpg);}

/*顶过滤条*/
.Filters{ clear:both; background-color:#fff; height:40px; padding:10px; margin-bottom:20px; border-bottom:2px solid #eaeaea;}
.Filters .text-container{ float:right; width:160px;}
.Filters input{ float:right; border:0; height:40px; background-color:#ebf3ff;}
.Filters .submit{ width:30px; cursor:pointer; background:url(../../../images/v2/icons/magnifier-l.png) #ebf3ff center no-repeat;}
.Filters .submit{ _background-image:url(../../../images/v2/icons/magnifier-l.gif);}
.Filters .text{ padding:0 10px; font-size:16px; color:#1562c8; width:140px; line-height:40px;}
.Filters .SELECT{ float:right; margin-right:10px; width:130px;}
.Filters .SELECT dt{ height:40px; line-height:40px; background-color:#ebf3ff;}
.Filters .SELECT dt span,.Filters .SELECT dt b{ font-size:16px; width:95px;}
.Filters .SELECT dd{ border:2px solid #ebf3ff; font-size:14px;}
.Types dd{ width:126px;}
/*校企合作邀请-学校列表顶的特殊样式*/
.float-left{ background-color:#ebf3ff; padding:20px;}
.float-left .SELECT,.float-left input,.float-left .text-container{ float:left !important;}
.float-left .SELECT{ width:160px;}
.float-left .SELECT dt{ border:#c4ddff 1px solid; background:url(../../../images/v2/icons/map.jpg) 10px center no-repeat #fff;}
.float-left .SELECT dt span,.float-left .SELECT dt b{ width:85px; margin-left:25px;}
.float-left .SELECT dt i{ width:25px; height:20px; border-left:1px solid #c4ddff; background-repeat:no-repeat; background-position:center right;}
.float-left .Keywords{ margin-left:10px; float:left;}
.float-left .text-container{ width:350px;}
.float-left .text{ width:320px; background-color:#fff; border:1px solid #c4ddff;}
.float-left .submit{ width:140px; height:42px; background:#1562c8; font-size:18px; color:#fff; margin-left:10px;}


h2.Professions{ background-color:#f2f2f2; color:#525252; font-weight:normal; line-height:40px; font-size:20px; padding:0 10px;}
div.Professions{ clear:both; border:1px solid #ededed; background-color:#f2f2f2;}
/*学校列表-校企合作邀请用*/
.Schools{}
.Schools li{ background-color:#fff; height:90px; padding:20px 0 20px 15px; margin-top:10px; border-bottom:1px solid #e8e8e8;}
.Schools li var{ float:left; display:block; width:88px; height:88px; border:1px solid #e7e7e7; overflow:hidden; margin-right:15px;}
.Schools li a{ color:#1562c8;}
.Schools li h3{ width:555px; float:left; white-space:nowrap; overflow:hidden; text-overflow:ellipsis; line-height:30px; font-weight:normal;}
.Schools h3 a:hover{ text-decoration:underline;}
.Schools li q{ width:100px; height:25px; line-height:25px; float:right; font-size:16px; text-align:center;}
.Schools li q samp{ background-color:#f2f2f2; color:#a4a4a4; display:block;}
.Schools li q a{ display:block; background-color:#fcde4e;}
.Schools q a:hover{ background-color:#1562c8; color:#fff;}
.Schools li p{ width:600px; float:left; color:#525252; font-size:14px; padding:5px 0; line-height:25px;}
.Schools li span{ color:#a4a4a4;}

.PAGES{ text-align:right; padding:20px 0;}

/*学校详情*/
.School{ clear:both; width:800px; margin:40px auto; background:#fff url(../../../images/v2/fold3.jpg) right top no-repeat; border-top:1px solid #f8f8f8; padding-bottom:100px;}
.TOPIC{ height:58px; position:relative; left:-10px; margin:25px 0 0px; line-height:46px; color:#fff; font-weight:normal; font-size:24px;}
.TOPIC *{ display:block; float:left; height:58px;}
.TOPIC b,.TOPIC q{ background-image:url(../../../images/v2/topicLong.png); width:30px;}
.TOPIC b,.TOPIC q{ _background-image:url(../../../images/v2/topicLong.gif);}
.TOPIC q{ background-position:right top; width:50px;}
.TOPIC strong{ background-color:#71a4e7; height:48px;}
/*相册*/
.Albums{ clear:both; margin:10px 25px; height:330px;}
.Albums dt{ width:290px; height:290px; overflow:hidden; background-color:#f8f8f8; float:left; margin-top:10px;}
.Albums dd{ width:450px; float:left;}
.Albums dd li{ width:140px; float:left; margin:10px 0 0 10px; height:140px; background-color:#f8f8f8; overflow:hidden; cursor:pointer;}
/*小标题*/
.School h2{ height:20px; font-size:20px; color:#1562c8; border-left:3px solid #1562c8; padding-left:10px; line-height:20px; margin:0 20px;}
.Content{ padding:20px; font-size:16px; color:#525252; line-height:30px;}
.Content p{ text-indent:2em; margin-bottom:10px;}

/*尾巴上的静止层*/
.Tail{ width:100%; text-align:center; padding:10px 0; line-height:40px;}
.Tail a{ display:inline-block; background-color:#1562c8; color:#fff; font-size:24px; padding:0 20px; border:2px solid #1562c8;}
.Tail a:hover{ background-color:#fff; color:#1562c8;}
.Tail span{ display:inline-block; background-color:#a4a4a4; color:#525252; font-size:24px; padding:0 20px;}

/*学校列表-发出/收到的邀请用*/
.Schools2{}
.Schools2 li{ background-color:#fff; height:105px; margin-top:10px; border-bottom:1px solid #e8e8e8;}
.Schools2 h3{ width:600px; float:left; white-space:nowrap; text-overflow:ellipsis; line-height:35px; font-weight:normal; margin-top:5px;}
.Schools2 h3 a{ color:#525252;}
.Schools2 small{ font-size:16px;}
.Schools2 var{ display:block; width:100px; height:100px; float:left; border:1px solid #f8fbff; overflow:hidden; margin:1px 10px 0 1px;}
.Schools2 h3 a:hover{ color:#1562c8; text-decoration:underline;}
.Schools2 q{ width:300px; height:25px; line-height:25px; float:right; font-size:16px; text-align:right; margin-top:35px; padding:0 10px;}
.Schools2 q a{ color:#1562c8; padding:0 10px;}
.Schools2 q a:hover{ text-decoration:underline;}
.Schools2 p{ width:350px; float:left; color:#a4a4a4; font-size:16px; line-height:30px;}
.Schools2 b{ font-weight:normal; color:#525252;}

/*新增的详细内容列表*/
.Schools2 li.on{ height:auto;}
.Schools2 q a i{ display:inline-block; width:12px; height:12px; margin-left:3px; background-image:url(../../../images/v2/icons/triangle-down-blue-deep.png);}
.Schools2 q a i{ _background-image:url(../../../images/v2/icons/triangle-down-blue-deep.gif);}
.Schools2 li.on q a i{ background-image:url(../../../images/v2/icons/triangle-up-blue-deep.png); _background-image:url(../../../images/v2/icons/triangle-up-blue-deep.gif);}
.Schools2 div.sublist{ display:none; clear:both; background-color:#e8f2ff; padding:20px; line-height:40px;}
