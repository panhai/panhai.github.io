.Schools2 .sublist label input{ display:none;}
.Schools2 .sublist .submit{ width:80px; height:26px; font-size:14px; color:#fff; background-color:#62a5fe; cursor:pointer; border:0; margin-left:20px;}

/*地区推荐*/
.Pushes{ clear:both; margin:20px 0; border:1px solid #e6e6e6; background-color:#fff;}
.Pushes dt{ font-size:20px; line-height:45px; height:45px; background-color:#f2f2f2; padding:0 10px;}
.Pushes dt b{ color:#1562c8; font-weight:normal;}
.Pushes dt small{ font-size:14px; color:#8b8b8b;}
.Pushes dd{ padding:10px 0; line-height:40px; font-size:0; padding-bottom:20px\9;}
.Pushes dd a{ display:block; float:left; text-align:center; height:40px; color:#3f3f3f; width:157px; background:url(../../../images/v2/icons/rect-bg-gray.png) center no-repeat; _background-image:url(../../../images/v2/icons/rect-bg-gray.gif); margin-bottom:-10px\9;}
.Pushes dd a:hover{ background-image:url(../../../images/v2/icons/rect-bg-blue.png); _background-image:url(../../../images/v2/icons/rect-bg-blue.gif);}
.Pushes dd a span{ display:block; margin:0 auto; width:110px; white-space:nowrap; overflow:hidden; text-overflow:ellipsis; font-size:14px;}

/*校企合作列表*/
dl.Professions{ clear:both; margin-bottom:50px;}
.Professions .tabs{ height:45px; border-bottom:2px solid #77b0fb; overflow:hidden;}
.Professions h2{ float:left; width:165px; text-align:center; font-size:20px; color:#77b0fb; line-height:45px; font-weight:normal; border:1px solid #77b0fb; margin-right:5px; cursor:pointer;}
.Professions h2.cur{ color:#fff; background-color:#77b0fb;}
.Professions .contents{ border:1px solid #e6e6e6; background-color:#fff; border-top:0;}
/*技能培训机构*/
.Skills{ width:100%;}
.Skills td,.Skills th{ vertical-align:top;}
.Skills th{ background-color:#f2f2f2;}
.Skills th h3{ padding:0 20px; text-align:left; font-weight:normal; color:#5c5c5c; font-size:16px; line-height:60px; border-bottom:1px solid #e6e6e6; background:url(../../../images/v2/icons/chevrons-right-gray.png) 190px center no-repeat; border-right:1px solid #e6e6e6; cursor:pointer;}
.Skills th h3{ _background-image:url(../../../images/v2/icons/chevrons-right-gray.gif);}
.Skills th h3.cur{ background:#fff; color:#1562c8; border-right:0;}
.Skills .lists{ clear:both;}
.Skills .lists div{ clear:both; margin:10px 20px;}
.Skills h4{ height:40px; line-height:40px; border-bottom:1px solid #e1e1e1;}
