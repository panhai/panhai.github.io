/*二维码*/
.Code{ width:204px; height:106px; border:1px solid #c8c8c8; float:right; line-height:28px; font-size:16px; color:#525252;}
.Code i{ display:block; float:left; width:96px; height:106px; text-align:center; overflow:hidden; text-align:center; font-size:12px; overflow:hidden; line-height:20px; font-style:normal;}
.Code i img{ margin-top:8px;}
