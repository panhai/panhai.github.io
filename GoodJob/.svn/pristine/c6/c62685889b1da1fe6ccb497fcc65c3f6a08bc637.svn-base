.Perfect .Perfect-sex label { display: block; float: left; width: 90px; line-height: 28px; text-align: center; height: 28px; background-color: #fff; border: 1px solid  #eeebeb; width: 91px; cursor: pointer; }
.Perfect .Perfect-sex label.checked { border-color: #67a8fc; background: url(../../images/v2/icons/checked-b.png) right bottom #e8f2ff no-repeat; _background: url(../../images/v2/icons/checked-b.gif) right bottom #e8f2ff no-repeat; }
