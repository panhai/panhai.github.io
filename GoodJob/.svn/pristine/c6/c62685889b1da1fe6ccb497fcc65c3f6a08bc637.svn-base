/* 职位详情页 */
.HEAD{  background-color: #fff;}
body{ background-color: #f8fbff;}
.Main{ margin-top:20px; margin-bottom:20px;}
.Left{ width:740px; float:left; background-color:#ffffff;}
.Right{ width:300px; float:right;}

/*顶部详细信息*/
.Attribs{ clear:both; padding:20px 42px 15px; }
.Attribs dt{ min-height:60px; _height:60px;}
.Attribs dt.short{ height:105px;}
.Attribs .topic{ float:left; width:580px; overflow:hidden; padding-top:10px; _height:52px;}
.Attribs .hover{ _height:auto; overflow:visible;}
.Attribs h1{ font-weight: normal;font-size:28px; color:#333333; margin-right:15px; line-height:30px; padding:11px 0; float:left; white-space:nowrap; overflow:hidden; max-width:16em; text-overflow:ellipsis;}
.Attribs h2{ font-size:30px; line-height:70px; height:50px; float:left; color:#1562c8; overflow:hidden; width:580px; text-overflow:ellipsis; white-space:nowrap;}
.Attribs h1:hover,.Attribs .hover h1{ white-space:normal;}
.Attribs small{ display:block; float:left; padding-left:35px; height:35px; overflow:hidden; background:url(../../images/v2/icons/push.png) left top no-repeat; line-height:35px; margin:10px 0 0 0; font-size:20px; font-family:Arial, Helvetica, sans-serif; color:#ddac15; line-height:50px; font-weight:bold;}
.Attribs small{ _background-image:url(../../images/v2/icons/push.gif);}
.Attribs .icons{ display:block; float:left; margin:20px 0 0 10px; width:26px; height:26px;}
.Attribs .share{ background-image:url(../../images/v2/icons/share.png); _background-image:url(../../images/v2/icons/share.gif);}
/*分享条*/
.share cite{ left: -58px; top:34px; width:125px; height: 144px; border:1px solid #c5dbf9;}
.share cite i{ width:14px; height:8px; left:62px; top:-9px; background-image:url(../../images/v2/share/angle-top.jpg); _background-image:url(../../images/v2/share/angle-right.gif); _overflow:hidden;}
.share cite span{ width: 100%;display:inline-block; float: left;clear:both; height: 36px;text-align:left; color: #555555; font-size: 14px; font-style: normal; line-height:36px;}
.share cite span:hover{ background:#c4daf9;}
.share cite span em{ width: 48px; height: 21px; display: inline-block; vertical-align: text-bottom;}
.share cite .bds_tsina em{background:url('../../images/v2/share/sina-n.png') no-repeat center center; }
.share cite .bds_tqq em{background:url('../../images/v2/share/tencent-n.png') no-repeat center center; }
.share cite .bds_qzone em{background:url('../../images/v2/share/qzone-n.png') no-repeat center center; }
.share cite .bds_sqq em{background:url('../../images/v2/share/qq-n.png') no-repeat center center; }
.share cite .bds_tsina:hover em{background:url('../../images/v2/share/sina-on.png') no-repeat center center; }
.share cite .bds_tqq:hover em{background:url('../../images/v2/share/tencent-on.png') no-repeat center center; }
.share cite .bds_qzone:hover em{background:url('../../images/v2/share/qzone-on.png') no-repeat center center; }
.share cite .bds_sqq:hover em{background:url('../../images/v2/share/qq-on.png') no-repeat center center; }
.Attribs dd{ line-height:40px; font-size:16px; color:#a4a4a4;}
.Attribs b{ color:#525252;}
.Attribs b i{ color:#f7831c; font-style:normal;}
.Attribs var{ color:#f7831c; font-size:20px; font-style:normal;}
.Attribs code{ display:inline-block; font-family:"微软雅黑"; color:#525252;}
.WELFARE u,.WELFARE s{ color:#5593e6; background-image:url(../../images/v2/tag2.png); _background-image:url(../../images/v2/tag2.gif);}
.Attribs kbd{ display:block; float:left; color:#5593e6; font-size:14px; font-weight:normal; line-height:30px; margin-bottom:20px;}
/*公司地址*/
.Attribs .INFO{ font-size: 14px; line-height: 30px;}
/*公司福利*/
.Attribs  .WELFARE-MORE{line-height:48px; display: inline-block;margin-top: 12px;font-size: 14px; color:#70a7f1; padding-right: 18px;  background: url(../../images/v2/icons/chevrons-down.png) right center no-repeat;  cursor: pointer;}
.Attribs .WELFARE-MORE.on{background-image: url("../../images/v2/icons/chevrons-up.png");}
.WELFARE{ width: 570px; height: 50px; overflow:hidden; float: left;}
.WELFARE span{ margin-top: 18px; margin-right: 10px;width: 100px; height:28px;background:#f0f6ff;color:#999;font-size:14px;line-height:28px;text-align:center;}
/*收藏提示*/
.Attribs .favor{ background-image:url(../../images/v2/icons/favor.png); _background-image:url(../../images/v2/icons/favor.gif);}
.Attribs .favored{ background-image:url(../../images/v2/icons/favor2.png); _background-image:url(../../images/v2/icons/favor2.gif);}
.Attribs .tip{ display:none; float:right; background-color:#d1e3fc; line-height:34px; height:34px; overflow:hidden; color:#525252; font-size:0px; padding:0px 10px; white-space:nowrap; text-decoration:none;}
.Attribs .tip q,.Attribs .tip span{ display:inline-block; vertical-align:middle;}
.Attribs .tip span{ font-size:14px;}
