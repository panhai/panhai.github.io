<script type="text/javascript">
    //点击已看
    $(".Resume-operate a").click(function(){
        if($(this).index() == 0)
          $(this).addClass('not').text("已看");
    });

    $(".Main").delegate("label","click",function(event){

        if(event.target.nodeName == "LABEL"){
            if($(this).hasClass('check')){
                $(this).removeClass('check').find("input")[0].checked = false;
            }else{
                $(this).addClass('check').find("input")[0].checked = true;
            }
        }
        setCs();
        return false;
    });


    $(".all label").click(function(){
        var _this = this;
        setTimeout(function(){
            var bool = $(_this).hasClass("check"),
                    attr = bool ? "addClass":"removeClass";
            $(".resumeInfo").find("label").each(function(idx,el){
                $(this)[attr]("check").find("input").prop("checked",bool);
            });
            setCs();
        },10)

    });
    var array = [];
    $(".Info-item:odd").css("background","#f4f9ff");
    function setCs(){
        var    ipt =  $(".resumeInfo").find("input:checked"),
            length =  ipt.length,
                id =  ipt.attr('data-id');


        $(".all").find("a").attr({'data-length': length,'data-meny':length+".0"});
    }

    $(".all a").click(function(){
        var id = [];
        if(!$(".resumeInfo").find("input:checked").length == 0){
        $("input:checked").each(function(){
               id.push($(this).data('id'));
        })
            $(".all").find("a").attr("data-id",id)
            console.log(id)
            CENTERDIV.open.call($(".DownResume").get(0))
        }else{
            alert("不能为空")
            return false;
        }
    })


    $(".text").focus(function() {
         $(this).closest("td").find('em').show();
    }).focusout(function(){
        var _this = this;
        setTimeout(function () { $(_this).closest("td").find('em').hide(); }, 100);
    })
    $(".text").siblings('em').click(function(event) {
        $(this).siblings('.text').val("")
    });


</script>
