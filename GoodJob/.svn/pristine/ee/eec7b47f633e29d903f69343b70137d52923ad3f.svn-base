<script>

var Templates={
	Interview:[
		{
			id:1
			,title:'面试邀请一'
			,content:'面试邀请一面试邀请一面试邀请一面试邀请一面试邀请一面试邀请一面试邀请一面试邀请一面试邀请一面试邀请一面试邀请一面试邀请一面试邀请一'
		},
		{
			id:2
			,title:'面试邀请二'
			,content:'The second interview invitation...'
		}
	],
	Notmatch:[
		{
			id:10
			,title:'不合适的信'
			,content:'对不起，您对我们的职位不太合适……'
		},
		{
			id:12
			,title:'拒绝面试'
			,content:'拒绝面试的信'
		}
	]
},
Profiles={
	1:{
		email:'一号的邮箱',
		name:'第一个人',
		tel:'020-11111111',
		add:'一号简历的地址'
	},
	2:{
		email:'邮箱二',
		name:'老二',
		tel:'020-020202020',
		add:'某处'
	},
	3:{
		email:'三',
		name:'三',
		tel:'020-3333',
		add:'333'
	}
}
$(function(){
	LeftMenu();
    ToolPull();
	SelectJobs();
	ClearBorder();
	CenterDiv();
	TimePicker();
	FillTemplates("Interview,Notmatch");
	CombindCenterDiv('.resumes ins a:first-child','.Interview')
	CombindCenterDiv('.resumes ins a:last-child','.Notmatch')
	
	$('.resumes ins a:first-child').click(function(){
		var id=$(this).data("id");
		if(Profiles[id])
		{
			var pro=Profiles[id];
			$(".profile-email").html(pro.email);
			$(".profile-name").val(pro.name);
			$(".profile-tel").val(pro.tel);
			$(".profile-add").val(pro.add);
		}
		else
		{
			alert("资料不存在");
			return false;
		}
	});
});
</script>