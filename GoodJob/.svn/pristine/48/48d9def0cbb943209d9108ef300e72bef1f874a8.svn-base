/* 高级搜索 */
.CRUMB{ margin-top:30px;}
.Main{ padding-top:15px;}
.Left{ width:745px; float:left; margin-bottom:30px;}
.Right{ width:280px; float:right; z-index:1\9; position:relative; _position:static;}
.RightFixed{ width:280px;}
.bottom0{ position:absolute; bottom:0;}
/*过滤器组*/
.Filters{ padding-bottom:15px; line-height:30px; z-index:2\9; position:relative\9;}
.Filters table{ width:100%; border:2px solid #e8f2ff;}
.Filters th,.Filters td{ padding:8px 0px; vertical-align:top; border-bottom:1px solid #e8f2ff;}
.Filters thead th,.Filters thead td{ border-bottom-width:2px;}
.Filters thead td{ font-size:16px;}
.Filters th{ font-size:14px; color:#525252; font-weight:normal; background-color:#f2f8ff; text-align:right;}
.Filters tbody td{ font-size:14px;}
/*头部选项*/
.Filters thead a{ display:inline-block; padding:0px 10px; font-size:16px; color:#525252; margin:0 10px 5px 0px; background-color:#e8f2ff; float:left;}
.Filters thead a:hover{ background-color:#1562c8; color:#fff;}
/*普通选项*/
.Filters .items{ width:860px; height:30px; float:left; overflow:hidden; padding-left:10px;}
.Filters tbody label{ display:inline-block; white-space:nowrap; margin:0 0 5px 5px; float:left; width:105px; padding:0 5px; overflow:hidden; text-overflow:ellipsis; _height:20px; _padding:5px; _line-height:20px; color:#525252; font-size:0;}
.Filters tbody label.first{ width:105px; text-align:center; margin-left:0;}
.Filters tbody label.checked{ background-color:#1562c8;}
.Filters tbody label.checked a{ color:#fff;}
.Filters tbody input{ margin-right:3px;}
.Filters tbody a{ color:#525252; font-size:14px;}
.Filters tbody a:hover{ text-decoration:underline; color:#1562c8;}
.Filters tbody a.checked{ color:#fff;}
/*更多*/
.Filters var{ display:block; float:right; color:#4b89dc; font-style:normal; padding-right:15px; margin-right:5px; background:url(../../images/v2/icons/chevron-down.png) right center no-repeat; font-size:12px; cursor:pointer;}
.Filters tbody var,.Filters tfoot var{ _margin-right:2px;}
.Filters var{ _background-image:url(../../images/v2/icons/chevron-down.gif);}
.Filters thead var{ display:none;}
.Filters .last th,.Filters .last td{ border-bottom-width:2px;}
/*多重下拉列表*/
.Filters .container{ float:left;}
.Filters tfoot th.root,.Filters tfoot td.root{ height:40px; line-height:40px;}
.Filters .selects{ width:860px; float:left; padding-left:10px; height:40px; overflow:hidden;}
.Filters .SELECT{ float:left; border:2px solid #e8f2ff; margin:2px 5px 5px 0;}
.Filters .SELECT-on{ border-color:#b3d1f9 #b3d1f9 #fff #b3d1f9;}
.Filters .SELECT dt{ width:91px; line-height:30px; cursor:pointer;}
.Filters .SELECT dt i{ background-image:url(../../images/v2/icons/chevron-down-gray.png); _background-image:url(../../images/v2/icons/chevron-down-gray.gif);}
.Filters .SELECT-on dt{ z-index:2;}
.Filters .SELECT dt span,.Filters .SELECT dt b{ width:4.5em; font-size:16px; color:#525252;}
.Filters .SELECT-on dd{ border:2px solid #b3d1f9; z-index:1; top:30px;}
/*职位类别及行业类别定位*/
.filter-jobs dd,.Railway dd{ left:-104px;}
.filter-profs dd{ left:-224px;}
.Railway dd{ width:1056px;}
.Railway table{ border:0;}
.Railway td a{ display:inline-block; padding:0px 10px;}
.Railway td a:hover,.Railway td a.checked{ color:#1562c8; text-decoration:underline;}

.Filters .SELECT dd.normal{ width:111px; left:-2px;}
.Filters .SELECT dd.normal a{ font-size:14px; padding:0 10px; white-space:nowrap; overflow:hidden; text-overflow:ellipsis; clear:both; display:block; width:88px\9;}
/*职位*/
.Jobs{ width:100%; float:left;}
.Jobs li{ height:138px; overflow:hidden;}
.Jobs li.bg{ background-color:#f8fbff;}
.Jobs li:hover{ background-color:#fffdf3;}
.Jobs samp{ color:#f00; display:inline !important;}
/*职位信息*/
.job{ width:310px; float:left; margin-left:10px; _margin-left:5px;}
.job h4{ margin:20px 0 10px; font-size:20px; line-height:30px; font-weight:normal; _width:100%;}
.job h4,.company h5{ white-space:nowrap; overflow:hidden;}
.job h4 a,.company h5 a,.job h4 i,.company h5 i{ vertical-align:middle; display:inline-block;}
.job h4 a,.company h5 a{ color:#525252; margin-right:10px; white-space:nowrap; overflow:hidden; text-overflow:ellipsis;}
.job h4 a{ max-width:12em;}
.job h4 a:hover,.company h5 a:hover{ color:#1562c8; text-decoration:underline;}
.job h4 i{ background:url(../../images/v2/icons/push-s.png) left 3px no-repeat; font-size:12px; font-family:Arial, Helvetica, sans-serif; color:#ddac15; font-style:normal; padding-left:25px; line-height:40px; font-weight:bold; height:30px; overflow:hidden;}
.job h4 i{ _background-image:url(../../images/v2/icons/push-s.gif);}
.Jobs p{ font-size:14px; color:#a4a4a4; white-space:nowrap; overflow:hidden; text-overflow:ellipsis; line-height:30px; height:30px; clear:both;}
.Jobs b{ color:#525252; font-weight:normal;}
.Jobs .map{ padding-left:15px; background:url(../../images/v2/icons/mapmark-s.png) left center no-repeat;}
.Jobs .map{ _background-image:url(../../images/v2/icons/mapmark-s.gif);}
/*公司信息*/
.company{ width:390px; float:left; margin-left:30px;}
.company h5{ margin:20px 0 10px; font-size:18px; line-height:30px; font-weight:normal; float:left; _width:280px;}
.company h5 a{ max-width:14em;}
.company h5 i{ width:20px; background:url(../../images/v2/icons/check-s.png) left center no-repeat; height:30px;}
.company h5 i{ _background-image:url(../../images/v2/icons/check-s.gif);}
.company code,.company del{ display:block; line-height:26px; font-size:14px; text-align:right; float:right; margin:20px 15px 0 0; line-height:30px;}
.company code{ color:#87b8f8;}
.company del{ color:#a4a4a4; text-decoration:none;}
.company .WELFARE{ height:25px; overflow:hidden;}

/*当前选中过滤项*/
.FilterTags{ line-height:22px; display:table;}
.FilterTags div{ padding:10px 20px 5px; padding-bottom:10px\9; width:1020px; background-color:#e8f2ff; float:left; overflow:hidden;}
.FilterTags span{ display:inline-block; color:#525252; float:left; width:70px; overflow:hidden; font-size:16px;}
.FilterTags .only span{ width:auto;}

.FilterTags a{ display:inline-block; color:#4b89dc; background-color:#fff; padding:0px 10px; white-space:nowrap; margin:0 0 5px 5px; float:left; font-size:14px;}
.FilterTags a:hover{ background-color:#87b8f8; color:#fff;}
.FilterTags a q{ padding-left:5px; color:#acd0ff; font-family:Arial, Helvetica, sans-serif;}
.FilterTags samp{ float:right; font-size:14px; color:#000;/* display:inline-block; height:30px; margin-bottom:3px;*/}
.FilterTags b{ color:#1562c8;}

/*分页*/
.PAGES{ text-align:right; padding:10px 0 0; padding-top:5px\9;}

/*保障中心*/
.Protect{ border:1px solid #22b68e; background-color:#f8fbff; padding-top:10px;}
.Protect h5{ font-size:24px; text-align:center; color:#22b68e; font-weight:normal;}
.Protect p{ line-height:25px; font-size:14px; color:#22b68e; padding:5px 10px 15px 55px; background:url(../../images/v2/icons/check.png) 5px 7px no-repeat;}
.Protect p{ _background-image:url(../../images/v2/icons/check.jpg)}

/*过滤条上加城市选择器*/
.Filters .CitiesPicker{ border-width:1px; margin:0 5px 0 0; width:113px; _width:93px !important;}
.Filters .CitiesPicker dt{ height:28px; line-height:28px; padding-left:25px; background:url(../../images/v2/icons/mapmark2-s.png) 10px center no-repeat; _width:78px;}
.Filters .CitiesPicker dt span,.Filters .CitiesPicker dt b,.Filters .CitiesPicker dt input{ line-height:28px; height:28px; float:left; font-size:16px; width:65px; _width:60px;}
.Filters .CitiesPicker dt i{ _background-position:center 3px;}
.Filters .CitiesPicker dd{ left:-1px; border-width:1px; top:29px;}
.Filters .CitiesPicker dd a:hover{ color:#fff;}

/*没有信息*/
.Nothing{ clear:both; text-align:center; padding:50px;}
.Nothing h5{ font-size:24px; color:#d1e5ff; line-height:50px;}
.Nothing h6{ font-size:14px; color:#d1e5ff; font-weight:normal; width:370px; margin:0 auto; text-align:left;}
.Nothing h6 a{ float:right; color:#1562c8; text-decoration:underline;}

/*新增相关数据*/
.Something{ border:1px solid #f2f2f2; margin:60px auto 20px;}
.Something table{ width:1020px; margin:10px auto;}
.Something th,.Something td{ font-size:14px; border-bottom:1px dashed #f2f2f2; padding:10px 0; vertical-align:top; line-height:30px; text-align:left;}
.Something th{ width:80px; color:#525252; font-weight:normal;}
.Something td a{ color:#a4a4a4; display:inline-block; width:8em; overflow:hidden; text-overflow:ellipsis; white-space:nowrap; margin-right:1.2em; float:left;}
.Something td a:hover{ color:#1562c8; text-decoration:underline;}
.Something tfoot th,.Something tfoot td{ border-bottom:0; border-top:1px solid #f2f2f2;}