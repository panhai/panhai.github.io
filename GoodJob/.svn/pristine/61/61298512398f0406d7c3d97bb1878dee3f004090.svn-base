<script>

$(function(){
    $(".switch").click(function(event) {
        if(!$(this).hasClass('on')){
            $(this).addClass('on')
            .find("i").animate({"left": "22px"}, 200)
            .end()
            .find("span")
            .text('已开启');
            $(".delivery").show();
        }else{
            $(this).removeClass('on')
            .find("i").animate({"left": 0}, 200)
            .end()
            .find("span")
            .text('已关闭');
            $(".delivery").hide();
        }
    });


   

    $(".exclude").click(function(event) {
        event.stopPropagation();
        $(this).closest('li').next().toggleClass('hide');;
        
    });
});
</script>