<script>
var PPT;
$(function(){
	defaultLng=116.316169,defaultLat=40.005567;
	if(defaultLng && defaultLat)
	{
		$(".MAPTHUMB dt,#MAPCONTAINER").slideDown(function(){
			//直接把坐标写上去
			map.refresh();
			//生成地图查看器的效果
			BIGMAP.refresh();
			panorama.setPosition(new BMap.Point(defaultLng,defaultLat));
		});
	}
	else
	{
		$(".MAPTHUMB").hide();
	}
	//点击公司图片的查看效果
	$(".Pics li img").each(function(){
		$("<img>").attr({
			src:$(this).data("url"),
			title:this.alt
		}).appendTo($("<li>").appendTo($(".PPT ul")));
		$("<img>").attr({
			src:this.src,
			title:this.alt
		}).appendTo($("<li>").appendTo($(".PPT ol")));
	});
	if(isLow()) TextOverflow('h1',12);
	//导航的滚动监听，因为本页没有高度可变项，所以采用最低级的算法
	CHIfixed(".SubNav","top");
	var ANCHOR_ACTIVE=function(){
		$(this).addClass("cur").siblings().removeClass("cur");
	}
	$(".SubNav a").click(function(){ANCHOR_ACTIVE.call(this)});
	var IntroDtHeight=$(".Intros dt").outerHeight();
	var ANCHORS=$(".SubNav a").map(function(){
		var href=this.href,
			id=href.substr(href.indexOf('#')+1);
		return $("#"+id).offset().top-IntroDtHeight;
	}).get();
	function NAV_LISTEN()
	{
		var scrollTop=$(window).scrollTop();
		for(var n=0;n<ANCHORS.length;n++)
		{
			if(ANCHORS[n]>scrollTop) break;
		}
		ANCHOR_ACTIVE.call($(".SubNav a").get(n ? n-1 :0));
	}
		
	$(window).on('scroll load',function(){
		if(isLow())
		{
			clearTimeout(ANCHORS.timer);
			ANCHORS.timer=0;
			ANCHORS.timer=setTimeout(NAV_LISTEN,100);
		}
		else NAV_LISTEN();
	});
});
$(window).load(function(){
	CHIpros(".PPT,.Pics,.Attribs dt");
	////////////////////////////////////////////////////////////////////////////////////////生成图片查看器的效果
	setTimeout(function(){
		PPT=CHIppt(
			".PPT","ul li",
			{indices:".PPT ol",prev:".PPT sub",next:".PPT sup"},
			{auto:false},
			{
				show:function(oJQli){
					$(".PhotoViewer h6").html(oJQli.find("img").attr("title"));
				}
			}
		);
		$(".PhotoViewer").hide().css('visibility','visible');
		$(".Pics li").click(function(){
			CHIbg.open(function(){
				$(".PhotoViewer").fadeIn();
			});
			PPT.show($(this).index());
		});
		$(".PhotoViewer q").click(function(){
			$(".PhotoViewer").fadeOut(function(){CHIbg.close()});
		});
	},1000);
    $(".WELFARE").height(3*38);
    $(".WELFARE-MORE").click(function(){
        if(!$(this).hasClass('on')){
            var len = Math.ceil($(".WELFARE").find('span').length / 4);
            if(len >= 1){
                console.log(len)
                $(".WELFARE").height(38 * len);
            }
            $(this).addClass('on')

        }else{
            $(".WELFARE").height(3*38)
            $(this).removeClass('on')
        }
    });
});
</script>
