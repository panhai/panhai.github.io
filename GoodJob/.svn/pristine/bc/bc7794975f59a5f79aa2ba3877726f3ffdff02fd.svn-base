                <script type="text/javascript">
                    var JBResetorEditPersonInfo = new JBodyInputResetor({ objs: document.getElementById("personInfoContainer"), autoCloseMsg: true });
                    function tryStartEditPersonInfo() {
                        JBResetorEditPersonInfo.save();
                        $("#personInfoContainer").show();
                        $("#personInfoContainer").parent().find(".blockMain").first().hide();
                        JBValidatorContainer.closeMsg();
                    }
                    function tryCancelEditPersonInfo(obj) {
                        JBResetorEditPersonInfo.reset();
                        $("#personInfoContainer").hide();
                        $("#personInfoContainer").parent().find(".blockMain").first().show();
                    }
                    function tryUpdateEditPersonInfo(xmlhttpRespText, xmlhttpRespObj) {
                        JBResetorEditPersonInfo.save();
                        $("#personInfoContainer").hide();
                        $("#personInfoContainer").parent().find(".blockMain").first().show();
                        for (var i = 0; i < xmlhttpRespObj.data.length; i++) {
                            $("#personInfoContainer").parent().find(".blockMainTableOfDetail").first().find("td").eq(i).html(xmlhttpRespObj.data[i]);
                        }
                    }
                    function trySubmitEditPersonInfo(obj) {
                        var htmObj = $("#personInfoContainer").get(0);
                        var data = JB.formToJson(htmObj);
                        data.cmd = "editpersoninfo";
                        if (JBValidatorContainer.checkAllChildren(htmObj)) {
                            new JBodyCommonAjax({
                                obj: null,
                                data: data,
                                debug: false,
                                title: "修改个人信息",
                                loadingMsg: "<img src='/images/common/loading/5.gif' alt='' />",
                                afterSuccess: tryUpdateEditPersonInfo
                            });
                        }
                    }
                </script>
