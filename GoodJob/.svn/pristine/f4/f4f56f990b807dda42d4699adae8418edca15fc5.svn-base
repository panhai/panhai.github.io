    <script type="text/javascript">
        function checkAndGotoNextStep(currentStep) {
            if (JBValidatorContainer.checkAllChildren(document.getElementById("addStep" + currentStep + "Container"))) {
                $("#addStep" + currentStep + "Container").hide();
                $("#addStep" + (currentStep + 1) + "Container").show();
                setCurentStep(currentStep + 1);
            }
        }
        function checkAndGotoPrevStep(currentStep) {
            $("#addStep" + currentStep + "Container").hide();
            $("#addStep" + (currentStep - 1) + "Container").show();
            setCurentStep(currentStep - 1);
        }
        function setCurentStep(st) {
            var aray = ["1、职位信息", "2、职责及要求", "3、企业信息", "4、薪酬福利", "5、其他补充", "6、完成"];
