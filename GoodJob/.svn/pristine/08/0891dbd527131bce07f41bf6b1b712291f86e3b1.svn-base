
.resumeEditShowMore,.resumeEditCloseMore{
     background-color:#eee;
     width:530px;
     padding:8px;
}
.resumeEditCloseMore .btbt{
    padding-bottom:5px;
    border-bottom:dashed 1px #ccc;
}
.resumeEditShowMore .btbt a,.resumeEditCloseMore .btbt a{
    background:url(/images/icons/resume_menu.png) no-repeat;
    padding-left:30px;
}
.resumeEditShowMore .btbt a {
    background-position: 0px -177px;
}
.resumeEditCloseMore .btbt a {
    background-position: 0px -155px;
}

.resumeDetailJobTitle,.resumeDetailEduTitle{
    padding: 6px;
    padding-left: 5px;
    height: 24px;
    margin-left: 10px;
    margin-right: 18px;
}
.resumeDetailJobTitle1,.resumeDetailEduTitle1{
    border-top:dashed 1px #ddd;
    margin-top:10px;
}
.resumeDetailJobTitle .time,.resumeDetailEduTitle .time{
    width:138px;
    float:left;
    text-align:right;
    font-size:14px;
    font-weight:bold;
}
.resumeDetailJobTitle .companyName,.resumeDetailEduTitle .schoolName{
    float:left;
    margin-left:20px;
    padding-left:5px;
    padding-right:5px;
    background-color:#e0e0e0;
    font-weight:bold;
}
.resumeDetailPosTitle{
    padding: 6px;
    padding-left: 5px;
    padding-top:5px;
    height: 24px;
    margin-left: 170px;
    margin-right: 10px;
    margin-top:5px;
    width:760px;
    border-top:dashed 1px #ddd;
    float:left;
}
.resumeDetailPosTitle .posName{
    float:left;
    font-weight:bold;
    padding-left:5px;
    padding-right:5px;
    background-color:#e0e0e0;
}
.resumeDetailPosTitle .time {
    float:left;
    font-weight:bold;
    margin-left:10px;
}

.resumePercent{
    width:100px;
    height:20px;
    border:solid 1px #0094ff;
    background-color:#ddd;
    overflow:hidden;
}
.resumePercent div{
    height:20px;
    line-height:20px;
    display:block;
    overflow:hidden;
    background-color:#0094ff;
}
.resumePercent span{
    position:relative;
    left:0px;
    top:-20px;
    display:block;
    width:100px;
    height:20px;
    line-height:20px;
    text-align:center;
    color:white;
}
