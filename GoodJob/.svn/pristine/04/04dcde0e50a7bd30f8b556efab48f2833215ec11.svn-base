/* 整站通用样式 */
*{ margin:0px; padding:0px; font-family:Microsoft YaHei, Arial;word-break: break-all;}
ul,ol,li{ list-style:none;}
q{ quotes:none;}
a{ text-decoration:none; cursor:pointer;}
a img{ border:0px;}
.common{ width:1000px; margin:0 auto; clear:both;}
body{}
input::-ms-clear{ display:none;}
:-moz-placeholder{ color:#999;}
::-moz-placeholder{ color:#999;}
:-ms-input-placeholder{ color:#999 !important;}
:focus{ outline:none;}
textarea{ resize:none !important;}
.clear{ clear:both !important; float:none !important;}
table{ border-collapse:collapse; table-layout:fixed;}
/*IE6 用absolute模拟fixed*/
body{
    _background-attachment: fixed;     /* prevent screen flash in IE6 确保滚动条滚动时，元素不闪动*/
}

.cursor{
    color: #ff6000!important;}
/*头部*/
.header{
    height: 50px;
    color: #333;
    font-size: 16px;
    line-height: 50px;
}
.header img{
    vertical-align: middle;
}
.header a,
.header span{
    color: #333333;
}
.header .cur,.cur{
    color: #04baea;
}
.header .header-left{
    float: left;
}
.header .header-right{
    float: right;
}
.header .header-cb{
    color: #333;
    margin-right: 30px;
}
.header .header-cb span{
    color: #49b4d2;
}
.header .header-sy{
    margin-right: 60px;
}

/*banner*/

.banner{
    width: 100%;
    height: 573px;
    overflow: hidden;
    background: url("../img/banner.jpg") no-repeat center top;
}
.banner .banner-btn{
    width: 600px;
    text-align: center;
    margin: 295px auto 0 auto;
}
.banner .banner-btn span{
    display: inline-block;
    width: 186px;
    height: 50px;
    margin-top: 60px;
    cursor: pointer;
}
.banner .banner-btn .banner-btn-bm{
    margin-left: -134px;
    margin-right: 14px;
}
.banner .view-gz{
    display: block;
    width: 450px;
    margin-top: 10px;
    font-size: 14px;
    color: #666666;
    text-decoration: underline;
}

/*右边广告*/

.rightAD{
    position: absolute;
    width: 300px;
    height: 320px;
    top: 622px;
    right: 10px;
    z-index: 222;
}
.rightAD span{
    position: absolute;
    right: 42px;
    top: 0;
    width: 30px;
    height: 30px;
    cursor: pointer;
}

.rightAD.fixed{
    position: fixed;
    top: 0;
    right: 10px;
}
/*报名流程*/
.CenterGZ{
    width: 924px;
    height: 555px;
    position: relative;
}
.CenterGZ .CENTERCLOSE{
    position: absolute;
    display: block;
    right: 22px;
    top: 22px;
    width: 30px;
    height: 30px;
    background: #00b0df url(../../../images/v2/icons/times-l.png) center no-repeat;
    _background-image: url(../../../images/v2/icons/times-l.gif);
    cursor: pointer;
}
.show-main{
    position: relative;
    width: 924px;
    margin: 0 auto;
}
.process{
    width: 874px;
    padding: 25px 0 50px;
    margin:0 auto;
}
.process .process-title{
    width: 394px;
    height: 42px;
    margin: 0 auto;
    text-align: center;
}
.process .process-title a{
    display: inline-block;
    width: 195px;
    height: 40px;
    float: left;
    background-color: #fff;
    font-size: 24px;
    line-height: 40px;
    color: #04baea;
    border: 1px solid #00b0df;
}
.process .process-title a.cur{
    background-color: #00b0df;
    color: #ffffff;
}

.process .process-item h2{
    padding-left: 38px;
    margin-bottom: 30px;
    font-size: 22px;
    color: #00b0df;
    font-weight: normal;
    background: url("../img/1.jpg") no-repeat left center;
}
.process .process-item2 h2{
    background: url("../img/2.jpg") no-repeat left center;
}
.process .process-item1 ul{
    height: 52px;
    margin-left: 36px;
    background: url("../img/process.jpg") no-repeat;
    line-height: 52px;
    font-size: 16px;
    color: #666666;
}
.process .process-item1 ul li{
    float: left;
    text-align: center;
}
.process .process-item1 ul .p1{
    width: 180px;
}
.process .process-item1 ul .p2{
    width: 295px;
}
.process .process-item1 ul .p3{
    width: 208px;
}
.process .process-item1 ul .p4{
    width: 136px;
}
.process .process-item span{
    color: #ff6000;
}
.process .process-item2,
.process .process-item3{
    margin-top: 30px;
    color: #666666;
    font-size: 16px;
    line-height: 36px;
}
.process .process-item3{
    margin-top: 26px;
}
.process .process-rule ul,
.process-item3 ul{
    float: right;
    width: 666px;
    clear: both;
    line-height: 36px;
}
.process-item3 ul{
    width: 670px;
}
.process .process-rule ul li.c{
    margin-bottom: 10px;
}
.process .process-rule a{
    color: #00b0df;
}
.process .process-rule p{
    width: 561px;
    margin: 20px auto 0;
    padding-left: 100px;
    font-size: 14px;
    color: #b2b1b1;
    line-height: 30px;
    clear: both;
}
.process .process-jl li{
    list-style-type: decimal;
}
.process .process-jl li.c{
    line-height: 30px;
}
.process .process-jl img{
    margin-left: 38px;
}
    /*优势介绍*/
.Advantage{
    width: 924px;
    margin: 40px auto 0;
}
.Advantage h2{
    text-align: center;
    font-weight: normal;
    font-size: 38px;
    color: #04baea;
}

.Advantage ul{
    height: 250px;
}
.Advantage li{
    width: 220px;
    float: left;
    text-align: center;
}
.Advantage li h3{
    font-weight: normal;
    font-size: 16px;
    margin: 14px 0;
}
.Advantage li p{
    font-size: 14px;
    line-height: 30px;
    text-align: left;
    text-justify: inter-word;
    color: #666666;
}
.Advantage li.bookmark{
    width: 220px;
    margin-left: 40px;
}
.Advantage li.yxlq{
    width: 236px;
    margin-left: 50px;
}
.Advantage li.offer{
    width: 250px;
    margin-left: 80px;
}
.Advantage hr{
    width: 924px;
    border:none;
    border-top:1px solid #f0f0f0;
    margin: 30px 0;
}
.Advantage .hr{
    margin: 35px 0;
}
.Advantage .banner-btn-bm{
    display: block;
    width: 250px;
    height: 50px;
    margin: 0 auto;
    text-align: center;
    line-height: 50px;
    color: #ffffff;
    font-size: 28px;
    background: url("../img/bm-bg.png") no-repeat;
    cursor: pointer;
}
.Advantage .cur{
    color: #ff6000;
}
    /*参赛者展示*/
.show-search{
    width: 923px;
    height: 44px;
    margin: 36px auto 36px;
    background: url("../img/search.jpg") no-repeat center top;
}
.show-search .show-search-ipt{
    width: 415px;
    height: 28px;
    float: right;
    padding-top: 8px;
}
.show-search  h2{
    float: left;
    margin-left: 66px;
    font-weight: normal;
    color: #ffffff;
    line-height: 44px;
    font-size: 24px;
}
.show-search .show-search-ipt input{
    float: left;
    height: 28px;
    border: 0;
}
.show-search .show-search-ipt input[type='text']{
    width: 325px;
    padding-left: 15px;
    background: #fff url("../img/search-left.jpg") no-repeat left top;
}
.show-search .show-search-ipt input[type='button']{
    width: 70px;
    background-color: #00b0df;
    font-size: 16px;
    color: #ffffff;
    cursor: pointer;
}

/*参赛着展示*/
.show-items{
    width: 214px;
    height: 385px;
    border: 1px solid #f2f2f2;
    float: left;
    margin-left: 20px;
    margin-bottom: 22px;
    cursor: pointer;
}
.show-items.hover{
    border-color: #00b0df;
}
.show-items h2{
    padding-left: 12px;
    height: 44px;
    line-height: 44px;
    background-color: #f2f2f2;
    font-size: 18px;
    font-weight: normal;
    text-align: left;
}
.show-items h2.top1{

    background: #f2f2f2 url("../img/top_05.png") no-repeat 12px center ;
}
.show-items h2.top2{

    background: #f2f2f2 url("../img/top_07.png") no-repeat 12px center ;
}
.show-items h2.top3{

    background: #f2f2f2 url("../img/top_09.png") no-repeat 12px center ;
}
.show-items h2 span{
    float: right;
    font-size: 12px;
    color: #999;
    margin-right: 10px;
}
.show-items h2 span i{
    font-size: 14px;
    color: #ff6000;
    font-style: normal;
}
.show-item-ticket{
    width: 120px;
    height: 120px;
    margin: 32px auto 14px;
    line-height: 85px;
    text-align: center;
    color: #fff;
    font-size: 12px;
}
.show-items .show-item-num{
    width: 127px;
    height: 25px;
    margin: 0 auto;
    line-height: 25px;
    font-size: 14px;
    text-align: center;
    color: #fff;
    /*background: #08a887;*/
    background: url("../img/ticket-bg.jpg") no-repeat;
}
.show-items .show-item-num span{
    font-size: 18px;
}
.show-item-info{
    padding-left: 12px;
}
.show-item-info h3{
    font-size: 24px;
    color: #333333;
    font-weight: normal;
    line-height: 36px;
    margin-top: 16px;
    width: 202px;
    overflow: hidden;
    white-space: nowrap;
}
.show-item-info h3 em{
    margin-left: 8px;
}
.show-item-info h3 em ,
.show-item-info h3 i{
    font-size: 14px;
    font-style: normal;
}
.show-items .show-item-info p{
    max-width: 12em;
    font-size: 14px;
    color: #666666;
    line-height: 28px;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}
.show-items .show-item-info p em{
    color: #999999;
    font-style: normal;
}

/*投票拉票*/
.show-item-vote{
    margin-top: 6px ;
    font-size: 16px;
    color: #ffffff;
    cursor: pointer;
}
 .show-item-vote span{
    display: inline-block;
    float: left;
    width: 50%;
    height: 32px;
    line-height: 32px;
    text-align: center;
}
.show-item-vote .show-item-tp{
    background-color: #ff6000;
}
.show-item-vote .show-item-lp{
    background-color: #fdad4e;
}

/*翻页*/
.page{
    overflow: hidden;

}
.page-tools{
    height: 35px;
    overflow: hidden;
    text-align: right;
    float: right;
    margin: 34px 33px 124px 0;
}
.page span{
    float: left;
    margin-right: 8px;
}
.page a{
 /*    width: 33px;
 height: 33px; */
    display: inline-block;
    margin-right: 8px;
    padding: 0 14px;
    border: 1px solid #00b0df;
    font-size: 12px;
    font-family: 宋体;
    line-height: 32px;
    color: #666666;
}
.page a.cur{
    color: #fff;
    background-color: #00b0df;
}
.page .page-next{
    width: 72px;
}

/*脚步*/
.footer{
    background-color: #f5f5f5;
}
.rule{ position: relative; border-top: 1px solid #efefef; }
.rule .block01 .text .p a{color: #666;}
.rule .block01 .text .p{ font-size:16px; color:#666; overflow:hidden; text-overflow:ellipsis; white-space:nowrap; padding-bottom:7px;}
.rule .block02{ padding:50px 50px 0 50px;}
.rule .block02 .item{ padding-bottom:40px;}
.rule .block02 .title{ font-size:20px; padding-bottom:20px; font-weight:bold;}
.rule .block02 .text{ font-size:14px; color:#999; line-height:24px;}
.rule .bottom{ height:190px; }
.rule .bottom .left{ float:left; color:#666; padding-top:40px;}
.rule .bottom .left .numbar{ font-size:40px; color:#04baea; font-weight:bold;}
.rule .bottom .right{ float:right; text-align:right; padding:50px 0 0 0;}
.rule .bottom .right .c-grey01{ color: #999999; font-size: 12px;}
.rule .bottom .right a{ display:inline-block; color:#666; padding:0 30px; border-right:1px solid #d4d4d4; font-size:14px;}

/*登录框*/
.CENTERDIV{
    display: block;
    border: 0;
}
.FullScreenPlugin .CenterTiny,.CenterVote{
    width: 452px;
    border: 0;
}
.FullScreenPlugin .CENTERTOPIC{
    background-color: #00b0df;
}
.CenterTiny .CENTERCONTENT-s{
    padding: 30px;
}
.CenterForm th, .CenterForm td{
    padding: 0;
}
.CENTERDIV  dd{
    background-color: #fff;
}
.CenterForm{
    margin: 0 auto;
}
.CenterForm .text{
    width: 290px;
    height: 38px;
    padding: 0;
    padding-left: 10px;
    margin-top: 0;
}
.CenterForm .Login-password{
    margin-top: 10px;
}

.CENTERDIV .submit, .CENTERDIV .submit:hover{
    background-color: #00b0df;
    border: 0;
}
.addition{
    font-size: 14px;
    color: #999999;
}
.addition a{
    color: #999999;
    float: right;
}
.CENTERBTNS a{
    margin-left: 20px;
}

/*完善简历*/
.CenterForm span{
    color: #333;
}
.Perfect .Perfect-text{
    font-size: 18px;
    color: #333333;
}
.Perfect a,.CenterVote  input{
    display: block;
    width: 132px;
    height: 38px;
    background-color: #00b0df;
    font-size: 16px;
    border:0;
    color: #ffffff;
    margin:24px auto;
    cursor: pointer;
    text-align: center;
}

/*邀请*/
.CenterInvi,
.Canvassing,
.CenterEnroll{
    width: 420px;
}
.Invitation dd,
.Canvassing dd,
.CenterNot dd,
.CenterVote dd,
.CenterEnroll dd
{
    padding: 26px 35px 28px 35px;
}

.Invitation-text, .Canvassing{
    font-size: 16px;
}
.CenterInvi input{
    width: 340px;
    height: 38px;
    padding-left: 10px;
    margin: 20px 0;
    border: 1px solid #00b0df;
}

/*拉票*/
.Canvassing img,
.CenterEnroll img{
    display: block;
    margin: 0 auto 30px;
}

.Canvassing-text{
    line-height: 38px;
}

/*投票*/

.CenterVote td{
    text-align: center;
}
.CenterVote p{
    text-align: center;
    font-size: 16px;
}
.CenterVote-text{
    color: #333;
    font-size: 24px;
}
.CenterVote td img{
    margin-top: 26px;
}
/*报名成功*/
.CenterEnroll h2{
    text-align: center;
    font-weight: normal;
    color: #ff701a;
}
.CenterEnroll img{
    margin: 20px auto;
}
/*已投票*/
.show-item-vote .not{
    background-color: #999;

}
/*报名未成功*/
.CenterNot{
    width: 480px;
    font-size: 18px;
    text-align: center;
    color: #333;
}
.CenterNot td{

    text-align: center;
}
.CenterNot p span{
    color: #ff701a;
}
.CenterNot td .CenterNot-t{
    font-size: 16px;
    color: #999999;
}
.CenterNot .Perfect-btn{
    display: block;
    width: 164px;
    height: 38px;
    line-height: 38px;
    background-color:#00b0df;
    color: #fff;
    font-size: 16px;
    border: 0;
    cursor: pointer;
    margin: 24px auto 20px;
}
/*ad*/
.CenterAD{
    position: relative;
    width: 617px;
    margin: 0 auto;
    background: none;
}
.CenterAD .CENTERTOPIC{
    background: none;
}

.CenterAD-close{
    display: block;
    position: relative;
    width: 40px;
    height: 40px;
    top: 40px;
    right: 88px;
    float: right;
    cursor: pointer;

}

.Warn dd{
    font-size: 18px;
}